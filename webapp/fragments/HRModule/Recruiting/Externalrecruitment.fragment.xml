<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns:u="sap.ui.unified" xmlns="sap.m" xmlns:tnt="sap.tnt"
	xmlns:fm="sap.ui.layout.form" xmlns:upload="sap.m.upload" xmlns:core="sap.ui.core">
	<VBox width="100%">
		<fm:SimpleForm layout="ColumnLayout" class="ssp-form-bg" columnsM="2" columnsL="2" columnsXL="4" singleContainerFullSize="true"
			labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:toolbar>
				<Toolbar class="ssp-title">
					<Button icon="sap-icon://nav-back" press="handleBackPress"/>
					<Title class="ssp-form-bg" text="{i18n>createservicerequest}"/>
					<ToolbarSpacer/>
				</Toolbar>
			</fm:toolbar>
			<fm:content>
				<Toolbar>
					<Title level="H3" text="{/ServiceDescription/}" class="ssp-tlbr-title"/>
					<ToolbarSpacer/>
				</Toolbar>
				<Label text="{i18n>serviceproduct}" required="true"/>
				<Input value="{/HRAppVisible/}" showSuggestion="" enabled="false" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>plant}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_COMMON_SRV-/A_Plant-com.swcc.Template.fragments.CommonF4.PlantF4-/PlantF4/" width="100%"
					placeholder="Enter Plant" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/PlantF4/}">
					<customData>
						<core:CustomData key="Plant" value="Plant"/>
						<core:CustomData key="PlantName" value="Plant Name"/>
					</customData>
				</Input>
				<!--	<Label text="{i18n>userid}" required="false"/>
				<Input enabled="false" value="{/LoginUserID}"/>-->
				<Label text="{i18n>jobtitle}" required="false"/>
				<Input ariaLabelledBy="-ZSSP_HR_SRV-/ZCDS_SF_JOBTITLE-com.swcc.Template.fragments.HRModule.JobtitleF4-/JobtitleF4/" width="100%"
					placeholder="Enter Job title" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/JobtitleF4/}">
					<customData>
						<core:CustomData key="keyfield" value="Job Code"/>
						<core:CustomData key="value_1" value="Job Desc"/>
					</customData>
				</Input>
				<!--	<Label text="{i18n>jobgrade}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_HR_SRV-/ZCDSV_SF_JOBGRADE-com.swcc.Template.fragments.HRModule.JobgradeF4-/JobgradeF4/" width="100%"
					placeholder="Select Job Grade" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/JobgradeF4/}">
					<customData>
						<core:CustomData key="keyfield" value="Job Grade"/>
						<core:CustomData key="value_1" value="Job Desc"/>
					</customData>
				</Input>-->
				<Label text="{i18n>yearsofexp}" required="true"/>
				<Input placeholder="Enter Job Number" type="Number" value="{/Recruitment/ExternalRecruitment/Header/Zexp/}"/>
				<!--	<Label text="{i18n>joblocation}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_HR_SRV-/ZCDSV_SF_JOBLOCATION-com.swcc.Template.fragments.HRModule.JoblocationF4-/JoblocationF4/" width="100%"
					placeholder="Enter Job Location" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/JoblocationF4/}">
					<customData>
						<core:CustomData key="keyfield" value="Job Code"/>
						<core:CustomData key="value_1" value="Job Location"/>
					</customData>
				</Input>-->
				<core:Title text=""/>
				<Label text="{i18n>Description}" required="true"/>
				<Input placeholder="Enter Description" value="{/ServiceDescription/}" enabled="false"/>
				<!--	<Label text="{i18n>jobdesc}" required="true"/>
				<Input placeholder="Enter Job Description" showSuggestion="" value="{/Recruitment/InternalRecruitment/Header/Zjdescr/}" showValueHelp=""
					valueHelpRequest=""/>-->
				<Label text="{i18n>thewebsite}" required="true"/>
				<Input placeholder="Enter Requirements and Conditions" value="{/Recruitment/ExternalRecruitment/Header/Zjdept/}" showSuggestion=""/>
				<Label text="{i18n>taskduties}" required="true"/>
				<Input placeholder="Enter Scope of Advertising" showSuggestion="" value="{/Recruitment/ExternalRecruitment/Header/Zjtasks/}"
					showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>qualificationreq}" required="true"/>
				<Input placeholder="Enter Requirements for Position" value="{/Recruitment/ExternalRecruitment/Header/Zjqreq/}" showSuggestion=""
					showValueHelp="" valueHelpRequest=""/>
				<!--	<Label text="{i18n>tasksrequired}" required="true"/>
				<Input placeholder="Enter Task" showSuggestion="" value="{/Recruitment/InternalRecruitment/Header/Zjtasks/}" showValueHelp=""
					valueHelpRequest=""/>-->
			</fm:content>
		</fm:SimpleForm>
	</VBox>
	<VBox width="100%">
		<fm:SimpleForm title="" layout="ResponsiveGridLayout" class="ssp-form-txtarea" columnsM="2" columnsL="3" columnsXL="4"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content>
				<Label text="{i18n>Remarks}" required="false"/>
				<TextArea placeholder="" rows="5" cols="20" growing="true" value="{/Recruitment/ExternalRecruitment/Header/Zcomment/}" growingMaxLines="7"
					height="6rem" width="100%"/>
			</fm:content>
		</fm:SimpleForm>
	</VBox>
</core:FragmentDefinition>