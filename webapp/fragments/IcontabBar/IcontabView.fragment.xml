<core:FragmentDefinition xmlns="sap.m" xmlns:tnt="sap.tnt" xmlns:fm="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">
	<VBox width="100%" class="ssp-tile sapUiMediumMarginTop">
		<fm:SimpleForm id="ssp-create-sr-7" title="{i18n>recentstatus}" layout="ColumnLayout" class="ssp-form-bg" columnsM="2" columnsL="3"
			columnsXL="3" singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content></fm:content>
		</fm:SimpleForm>
		<IconTabBar id="idIconTabBar" select="" class="sapUiResponsiveContentPadding">
			<items>
				<IconTabFilter showAll="true" count="" text="Products" key="All"/>
				<IconTabSeparator/>
				<IconTabFilter icon="sap-icon://open-folder" iconColor="Positive" count="" text="Open" key="Ok"></IconTabFilter>
				<IconTabFilter icon="sap-icon://customer-history" iconColor="Critical" count="" text="Inprogress" key="Heavy"></IconTabFilter>
				<IconTabFilter icon="sap-icon://folder-full" iconColor="Negative" count="" text="Closed" key="Overweight"></IconTabFilter>
