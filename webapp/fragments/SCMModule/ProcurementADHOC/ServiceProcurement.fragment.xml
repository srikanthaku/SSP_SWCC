<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns:u="sap.ui.unified" xmlns="sap.m" xmlns:tnt="sap.tnt"
	xmlns:fm="sap.ui.layout.form" xmlns:upload="sap.m.upload" xmlns:core="sap.ui.core">
	<VBox width="100%">
		<fm:SimpleForm layout="ColumnLayout" class="ssp-form-bg" columnsM="2" columnsL="2" columnsXL="4" singleContainerFullSize="true"
			labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:toolbar>
				<Toolbar class="ssp-title">
					<Button icon="sap-icon://nav-back" press="handleBackPress"/>
					<Title class="ssp-form-bg" text="{i18n>createservicerequest}"/>
					<ToolbarSpacer/>
				</Toolbar>
			</fm:toolbar>
			<fm:content>
				<Toolbar>
					<Title level="H3" text="{/ServiceDescription/}" class="ssp-tlbr-title"/>
					<ToolbarSpacer/>
					<!--<Button icon="sap-icon://settings" press=".handleButtonPress"/>-->
				</Toolbar>
				<Label text="{i18n>Serviceproduct}" required="true"/>
				<Input value="{/SCMAppVisible/}" showSuggestion="" enabled="false" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>Plant}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_COMMON_SRV-/A_Plant-com.swcc.Template.fragments.CommonF4.PlantF4-/PlantF4/" width="100%"
					placeholder="Enter Plant" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/PlantF4/}">
					<customData>
						<core:CustomData key="Plant" value="Plant"/>
						<core:CustomData key="PlantName" value="Plant Name"/>
					</customData>
				</Input>
				<Label text="{i18n>totalestimatedvalue}" required="true"/>
				<Input placeholder="" showSuggestion="" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>tenderpre}" required="true"/>
				<ComboBox items="">
					<core:Item key="Y" text="Yes"/>
					<core:Item key="N" text="No"/>
				</ComboBox>
				<Label text="{i18n>contractduration}" required="false"/>
				<Input placeholder="" showSuggestion="" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>projecttype}" required="false"/>
				<Input
					ariaLabelledBy="-ZSSP_SCM_SRV-/ZCDSV_DOMAINVH(p_domain='ZPROJ_TYP')/Set/-com.swcc.Template.fragments.SCMModule.ProjecttypeF4-/ProjecttypeF4/"
					width="100%" placeholder="Enter Project" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/ProjecttypeF4/}">
					<customData>
						<core:CustomData key="Status" value="Project type"/>
						<core:CustomData key="Text" value="Description"/>
					</customData>
				</Input>
				<!--	<ComboBox width="100%" valueState="None" placeholder="Select Project Type"
					selectedKey="{/ProcurementAdhoc/ServiceProcurement/Header/Status}" items="{/ProjectTypef4/}">
					<core:Item key="{Status}" text="{Status} - {Text}"/>
				</ComboBox>-->
				<Label text="{i18n>costestimationtype}" required="false"/>
				<Input ariaLabelledBy="-ZSSP_SCM_SRV-/ZCDSV_DOMAINVH(p_domain='ZEST_TYP')/Set/-com.swcc.Template.fragments.SCMModule.CostestF4-/CostestF4/"
					width="100%" placeholder="Enter Cost Estimation Type" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/CostestF4/}">
					<customData>
						<core:CustomData key="Status" value="Estimation Type"/>
						<core:CustomData key="Text" value="Description"/>
					</customData>
				</Input>
				<Label text="{i18n>technicalevaluationcriteria}" required="false"/>
				<Input
					ariaLabelledBy="-ZSSP_SCM_SRV-/ZCDSV_DOMAINVH(p_domain='ZTEC_EVA')/Set/-com.swcc.Template.fragments.SCMModule.TechnicalevalF4-/TechnicalevalF4/"
					width="100%" placeholder="Enter Technical Evaluation Criteria" valueHelpOnly="false" showValueHelp="true"
					valueHelpRequest="onValueHelpRequest" value="{/TechnicalevalF4/}">
					<customData>
						<core:CustomData key="Status" value="Evaluation Type"/>
						<core:CustomData key="Text" value="Description"/>
					</customData>
				</Input>
				<Label text="{i18n>suggestedcompany}" required="false"/>
				<Input placeholder="" showSuggestion="" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>Description}" required="true"/>
				<Input placeholder="Enter Description" value="{/ServiceDescription/}" enabled="false"/>
				<Label text="{i18n>extratermsconditions}" required="false"/>
				<Input placeholder="" showSuggestion="" showValueHelp="" valueHelpRequest=""/>
				<!--<Label text="{i18n>workcenter}" required="true"/>
				<Input placeholder="" showSuggestion="" showValueHelp="" valueHelpRequest=""/>-->
				<Label text="{i18n>totalestvalue}" required="true"/>
				<Input placeholder="" showSuggestion="" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>requeststat}" required="false"/>
				<Input
					ariaLabelledBy="-ZSSP_SCM_SRV-/ZCDSV_DOMAINVH(p_domain='ZREQ_STAT')/Set/-com.swcc.Template.fragments.SCMModule.RequeststatF4-/RequiredstatF4/"
					width="100%" placeholder="Enter Request Status" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/RequiredstatF4/}">
					<customData>
						<core:CustomData key="Status" value="Status"/>
						<core:CustomData key="Text" value="Text"/>
					</customData>
				</Input>
				<Label text="{i18n>confidentialproject}" required="false"/>
				<Input
					ariaLabelledBy="-ZSSP_SCM_SRV-/ZCDSV_DOMAINVH(p_domain='ZCONF_PROJ')/Set/-com.swcc.Template.fragments.SCMModule.ConfidentialF4-/ConfidentialF4/"
					width="100%" placeholder="Enter Confidential Project" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/ConfidentialF4/}">
					<customData>
						<core:CustomData key="Status" value="Status"/>
						<core:CustomData key="Text" value="Description"/>
					</customData>
				</Input>
				<Label text="{i18n>sitevisitrequired}" required="false"/>
				<Input
					ariaLabelledBy="-ZSSP_SCM_SRV-/ZCDSV_DOMAINVH(p_domain='ZSITE_VIS')/Set/-com.swcc.Template.fragments.SCMModule.SitevisitF4-/SitevisitF4/"
					width="100%" placeholder="Enter Required Site Visit" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/SitevisitF4/}">
					<customData>
						<core:CustomData key="Status" value="Required Site Visit"/>
						<core:CustomData key="Text" value="Description"/>
					</customData>
				</Input>
				<Label text="{i18n>economicfeasibilitystudy}" required="false"/>
				<Input
					ariaLabelledBy="-ZSSP_SCM_SRV-/ZCDSV_DOMAINVH(p_domain='ZECON_FEAS')/Set/-com.swcc.Template.fragments.SCMModule.ProjectstatusF4-/ProjectstatusF4/"
					width="100%" placeholder="Enter Plant" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/ProjectstatusF4/}">
					<customData>
						<core:CustomData key="Status" value="Status"/>
						<core:CustomData key="Text" value="Text"/>
					</customData>
				</Input>
				<Label text="{i18n>securityprojectapproval}" required="fasle"/>
				<Input
					ariaLabelledBy="-ZSSP_SCM_SRV-/ZCDSV_DOMAINVH(p_domain='ZSEC_PROJ')/Set/-com.swcc.Template.fragments.SCMModule.ProjectstatusF4-/ProjectstatusF4/"
					width="100%" placeholder="Enter Plant" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/ProjectstatusF4/}">
					<customData>
						<core:CustomData key="Status" value="Status"/>
						<core:CustomData key="Text" value="Text"/>
					</customData>
				</Input>
				<Label text="{i18n>justification}" required="false"/>
				<Input placeholder="" showSuggestion="" showValueHelp="" valueHelpRequest=""/>
			</fm:content>
		</fm:SimpleForm>
	</VBox>
	<VBox width="100%">
		<fm:SimpleForm title="" layout="ResponsiveGridLayout" class="ssp-form-txtarea" columnsM="2" columnsL="3" columnsXL="4"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content>
				<Label text="{i18n>Remarks}" required="false"/>
				<TextArea placeholder="" rows="5" cols="20" growing="true" growingMaxLines="7" height="6rem" width="100%"/>
			</fm:content>
		</fm:SimpleForm>
	</VBox>
	<VBox class="ssp-table-box">
		<!--<fm:SimpleForm id="ssp-create-sr-2" layout="ColumnLayout" class="ssp-form-bg" columnsM="2" columnsL="3" columnsXL="3"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content>-->
		<Toolbar>
			<Title level="H3" text="{i18n>itemoverview}" class="ssp-tlbr-title"/>
			<ToolbarSpacer/>
			<Button icon="sap-icon://add" press=".onAddItemsPress"/>
		</Toolbar>
		<ScrollContainer horizontal="true" vertical="false" height="auto">
			<Table mode="None" items="{path : '/MarineTransportation/itemData'}" sticky="ColumnHeaders,HeaderToolbar"
				class="ssp-request-Table sapUiContentPadding" inset="false" growingThreshold="5" growingScrollToLoad="true" growing="true"
				fixedLayout="true">
				<columns>
					<Column>
						<Text text="{i18n>servicecode}"/>
					</Column>
					<Column>
						<Text text="{i18n>serviceshort}"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="{i18n>servicegroup}"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="{i18n>purchasinggroup}"/>
					</Column>
					<Column width="5%"></Column>
				</columns>
				<items>
					<ColumnListItem vAlign="Middle">
						<cells>
							<Input placeholder="" showSuggestion="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequest"
								suggestionItems="{/ProductCollection}">
								<suggestionItems>
									<core:Item text="{Name}"/>
								</suggestionItems>
							</Input>
							<Text text="Will shi"/>
							<Input value="{BaseUnit}"/>
							<Text text="{uom}"/>
							<Input placeholder="" showSuggestion="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequest"
								suggestionItems="{/ProductCollection}">
								<suggestionItems>
									<core:Item text="{Name}"/>
								</suggestionItems>
							</Input>
							<Button icon="sap-icon://delete" press="onDeleteItemPress" type="Transparent"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</ScrollContainer>
		<!--	</fm:content>
		</fm:SimpleForm>-->
	</VBox>
	<VBox class="ssp-table-box">
		<!--<fm:SimpleForm id="ssp-create-sr-2" layout="ColumnLayout" class="ssp-form-bg" columnsM="2" columnsL="3" columnsXL="3"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content>-->
		<Toolbar>
			<Title level="H3" text="{i18n>itemdetail}" class="ssp-tlbr-title"/>
			<ToolbarSpacer/>
			<Button icon="sap-icon://add" press=".onAddItemsPress"/>
		</Toolbar>
		<ScrollContainer horizontal="true" vertical="false" height="auto">
			<Table mode="None" items="{path : '/MarineTransportation/itemData'}" sticky="ColumnHeaders,HeaderToolbar"
				class="ssp-request-Table sapUiContentPadding" inset="false" growingThreshold="5" growingScrollToLoad="true" growing="true"
				fixedLayout="true">
				<columns>
					<Column>
						<Text text="{i18n>shorttext}"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="{i18n>quantity}"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="{i18n>uom}"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="{i18n>estimatedunitprice}"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="{i18n>costcenter}"/>
					</Column>
					<Column width="5%"></Column>
				</columns>
				<items>
					<ColumnListItem vAlign="Middle">
						<cells>
							<Input placeholder="" showSuggestion="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequest"
								suggestionItems="{/ProductCollection}">
								<suggestionItems>
									<core:Item text="{Name}"/>
								</suggestionItems>
							</Input>
							<Text text="Will shi"/>
							<Input placeholder="" showSuggestion="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequest"
								suggestionItems="{/ProductCollection}">
								<suggestionItems>
									<core:Item text="{Name}"/>
								</suggestionItems>
							</Input>
							<Button icon="sap-icon://delete" press="onDeleteItemPress" type="Transparent"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</ScrollContainer>
		<!--	</fm:content>
		</fm:SimpleForm>-->
	</VBox>
	<VBox>
		<Table items="{/UploadedData}" class="upload-data" noDataText="{i18n>uploadquotation}">
			<headerToolbar>
				<Toolbar>
					<content>
						<FlexBox class="sapUiTinyMarginTop" alignItems="Start" justifyContent="SpaceBetween">
							<Title text="{i18n>Attachments}" level="H1"></Title>
							<Text text="{i18n>uploadtooltip}" class="sapUiTinyMarginBegin upload-info">
								<layoutData>
									<FlexItemData growFactor="1"/>
								</layoutData>
							</Text>
							<u:FileUploader class="sapUiContentPadding" buttonOnly="true" multiple="true" icon="sap-icon://attachment" visible="true"
								sameFilenameAllowed="false" buttonText="Upload file" change="onFileAdded" filenameLengthExceed="onFileNameLengthExceed"
								fileSizeExceed="onFileSizeExceed" maximumFileSize="2" uploadComplete="onFileComplete1" typeMissmatch="handleMissmatch"
								placeholder="Choose a file" style="Emphasized"
								mimeType="application/msword,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document"/>
						</FlexBox>
					</content>
				</Toolbar>
			</headerToolbar>
			<columns>
				<Column>
					<Text text="File Name"/>
				</Column>
				<Column>
					<Text text="File Size"/>
				</Column>
				<Column>
					<Text text="Delete"/>
				</Column>
				<!-- Add more columns as needed -->
			</columns>
			<items>
				<ColumnListItem>
					<cells>
						<Text text="{Filename}"/>
						<Text text="{path:'Filesize',formatter:'.formatBytesToMB'}"/>
						<Button type="Reject" text="Delete" press="onDeleteAttachment"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</VBox>
</core:FragmentDefinition>