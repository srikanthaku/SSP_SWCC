<core:FragmentDefinition xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified"
	xmlns:f="sap.ui.layout.form">
	<Dialog contentWidth="60%" contentHeight="80%" afterClose="onCloseSlaAgreementDialog">
		<ScrollContainer id="myScrollContainer" height="100%" width="100%" vertical="false">
			<core:HTML content=' &lt;embed type="application/pdf" id="myPDF" src="{/pdf}" width="100%" height="600px" /&gt; '/>
		</ScrollContainer>
		<endButton>
			<Button type="Reject" text="Close" press="onCloseAttachment"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>