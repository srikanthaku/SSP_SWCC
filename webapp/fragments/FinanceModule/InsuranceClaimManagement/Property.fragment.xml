<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:tnt="sap.tnt" xmlns:fm="sap.ui.layout.form"
	xmlns:upload="sap.m.upload" xmlns:u="sap.ui.unified" xmlns:core="sap.ui.core">
	<VBox width="100%">
		<fm:SimpleForm title="{i18n>createservicerequest}" layout="ColumnLayout" class="ssp-form-bg" columnsM="2" columnsL="3" columnsXL="4"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:toolbar>
				<Toolbar class="ssp-title">
					<Button press=".handleBackPress" icon="sap-icon://nav-back"/>
					<Title class="ssp-form-bg" text="{i18n>Property}"/>
					<ToolbarSpacer/>
				</Toolbar>
			</fm:toolbar>
			<fm:content>
				<!--<Toolbar>-->
				<!--	<Title level="H3" text="{i18n>ManagePettyCash}" class="ssp-tlbr-title"/>-->
				<!--	<ToolbarSpacer/>-->
				<!--<Button icon="sap-icon://settings" press=".handleButtonPress"/>-->
				<!--</Toolbar>-->
				<Label text="{i18n>serviceproduct}"/>
				<Input value="{/PMCreateRequest/Header/Material/}" showSuggestion="" enabled="false" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>quantity}" required="true"/>
				<Input placeholder="Enter Quantity" value="{/PMCreateRequest/Header/MaterialQty/}"/>
				<Label text="{i18n>plant}" required="true"/>
				<ComboBox width="100%" change="onCheckPlantVal" selectionChange="onSelectPlant" valueState="None" placeholder="Select Plant"
					selectedKey="{/PMCreateRequest/Header/Plant/}" items="{/PMCreateRequest/PlantF4/}">
					<core:Item key="{Plant}" text="{Plant} - {PlantName}"/>
				</ComboBox>
				<Label text="{i18n>workcenter}" required="true"/>
				<ComboBox width="100%" valueState="None" placeholder="Select Work Center" selectedKey="{/PMCreateRequest/Header/PmWkctr/}"
					items="{/PMCreateRequest/WorkCenterF4/}">
					<core:Item key="{Workcenter}" text="{Workcenter} - {WorkcenterDesc}"/>
				</ComboBox>
				<Label text="{i18n>Description}" required="false"/>
				<Input placeholder="Enter Description" value="{/ServiceDescription/}" enabled="false"/>
				<Label text="{i18n>shortdescription}" required="true"/>
				<Input placeholder="Allowed 40 Charcters" showSuggestion="" showValueHelp="" value="{/PMCreateRequest/Header/Descript/}" valueHelpRequest=""/>
				<Label text="{i18n>InsurancePolicyNo}" required="true"/>
				<Input required="true" width="100%" valueHelpOnly="true" showValueHelp="true" valueHelpRequest="onValueHelpRequest"/>
			</fm:content>
			<Button text="Search" type="Default" press="onSearchMngePttyCash" width="40%" class="sapUiTinyMarginBegin sapUiContentPadding swcc-pmy-btn"/>
		</fm:SimpleForm>
	</VBox>
	<VBox width="100%">
		<fm:SimpleForm title="" layout="ResponsiveGridLayout" class="ssp-form-txtarea" columnsM="2" columnsL="3" columnsXL="4"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content>
				<Label text="{i18n>Remarks}" required="false"/>
				<TextArea placeholder="" rows="5" cols="20" text="{/PMCreateRequest/Header/NotifText/}" growing="true" growingMaxLines="7" height="6rem"
					width="100%"/>
			</fm:content>
		</fm:SimpleForm>
	</VBox>
	<VBox width="100%">
		<fm:SimpleForm layout="ColumnLayout" class="ssp-form-bg" columnsM="2" columnsL="3" columnsXL="4" singleContainerFullSize="true"
			labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content>
				<Toolbar>
					<Title level="H3" text="{i18n>PropertyDetails}" class="ssp-tlbr-title"/>
					<ToolbarSpacer/>
					<!--<Button icon="sap-icon://settings" press=".handleButtonPress"/>-->
				</Toolbar>
				<Label text="{i18n>AssetNumber}"/>
				<Text text="2389798"/>
				<Label text="{i18n>SubNumber}"/>
				<Text text="8787867"/>
				<Label text="{i18n>AssetDesc}"/>
				<Text text="Aset Gluten"/>
				<Label text="{i18n>PropertyNumber}"/>
				<Text text="HBC-89788"/>
				<Label text="{i18n>District}"/>
				<Text text="Lambord"/>
				<Label text="{i18n>PurchaseAmount}"/>
				<Text text="IUI87787"/>
				<Label text="{i18n>YearofCapitalization}"/>
				<Text text="98777YTY"/>
				<Label text="{i18n>Plant}"/>
				<Text text="huiko"/>
			</fm:content>
		</fm:SimpleForm>
	</VBox>
	<VBox class="ssp-table-box">
		<!--<fm:SimpleForm id="ssp-create-sr-2" layout="ColumnLayout" class="ssp-form-bg" columnsM="2" columnsL="3" columnsXL="3"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content>-->
		<Toolbar>
			<Title level="H3" text="{i18n>Details}" class="ssp-tlbr-title"/>
			<ToolbarSpacer/>
			<Button icon="sap-icon://add" press=".onAddItemsPress"/>
		</Toolbar>
		<ScrollContainer horizontal="true" vertical="false" height="auto">
			<Table mode="None" items="{path : '/MarineTransportation/itemData'}" sticky="ColumnHeaders,HeaderToolbar"
				class="ssp-request-Table sapUiContentPadding" inset="false" growingThreshold="5" growingScrollToLoad="true" growing="true"
				fixedLayout="true">
				<columns>
					<Column>
						<Text text="Asset Number"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Sub Number"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Property Number"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Plant"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Claim Received Date"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Deductible Value"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Net Payment"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Insurance Policy No."/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Accident Date"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Payment Date"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Insurance Status"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Insurance Start Date "/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Insurance End Date"/>
					</Column>
					<Column width="5%"></Column>
				</columns>
				<items>
					<ColumnListItem vAlign="Middle">
						<cells>
							<Input placeholder="" showSuggestion="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequest"
								suggestionItems="{/ProductCollection}">
								<suggestionItems>
									<core:Item text="{Name}"/>
								</suggestionItems>
							</Input>
							<Input placeholder="" showSuggestion="" showValueHelp="" valueHelpRequest=""/>
							<Text text="Will shi"/>
							<Input placeholder="" showSuggestion="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequest"
								suggestionItems="{/ProductCollection}">
								<suggestionItems>
									<core:Item text="{Name}"/>
								</suggestionItems>
							</Input>
							<Input placeholder="" showSuggestion="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequest"
								suggestionItems="{/ProductCollection}">
								<suggestionItems>
									<core:Item text="{Name}"/>
								</suggestionItems>
							</Input>
							<Input value="SAR"/>
							<Input placeholder="" showSuggestion="" showValueHelp="" valueHelpRequest=""/>
							<Button icon="sap-icon://delete" press="onDeleteItemPress" type="Transparent"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</ScrollContainer>
		<!--	</fm:content>
		</fm:SimpleForm>-->
	</VBox>
	<VBox class="uploadBox sapUiSmallMarginBeginEnd">
		<Label text="{i18n>Attachments}" required="false"/>
		<u:FileUploader class="sapUiContentPadding" buttonOnly="true" multiple="true" icon="sap-icon://attachment" visible="true"
			sameFilenameAllowed="false" buttonText="Upload file" change="onUploadPress" filenameLengthExceed="onFileNameLengthExceed"
			fileSizeExceed="onFileSizeExceed" maximumFileSize="2" uploadComplete="onComplete" typeMissmatch="handleMissmatch"
			placeholder="Choose a file" style="Emphasized"
			mimeType="application/msword,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document"/>
	</VBox>
	<VBox>
		<Table class="upload-table" items="{/uploadedFiles}" mode="Delete" noDataText="{i18n>uploadtooltip}">
			<headerToolbar>
				<Toolbar>
					<content>
						<Image src="../webapp/util/Images/uploadfile.png" height="auto" width="auto">
							<layoutData>
								<FlexItemData growFactor="1"/>
							</layoutData>
						</Image>
					</content>
				</Toolbar>
			</headerToolbar>
			<columns>
				<Column>
					<Text text="File Name"/>
				</Column>
				<Column>
					<Text text="File Size"/>
				</Column>
				<Column>
					<Text text="Delete"/>
				</Column>
				<!-- Add more columns as needed -->
			</columns>
			<items>
				<ColumnListItem>
					<cells>
						<Text text="{FileName}"/>
						<Text text="{FileSize}"/>
						<Text text="{Delete}"/>
						<!-- Add more cells as needed -->
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</VBox>
</core:FragmentDefinition>