<mvc:View controllerName="com.swcc.Template.controller.FinanceViewRequest" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:f="sap.f"
	xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:fm="sap.ui.layout.form" xmlns:upload="sap.m.upload" height="100%">
	<f:DynamicPage showFooter="true" toggleHeaderOnTitleClick="false" class="ssp-page">
		<f:content>
			<l:VerticalLayout class="sapUiNoMargin sapUiNoContentPadding ssp-vtl" width="100%">
				<l:content>
					<core:Fragment
						fragmentName="com.swcc.Template.fragments.FinanceModule.InsuranceClaimManagement.InsuranceResgistration.View_InsuranceRegistration"
						type="XML"/>
				</l:content>
			</l:VerticalLayout>
		</f:content>
		<f:footer>
			<OverflowToolbar id="idOverflowToolbar" visible="true">
				<ToolbarSpacer id="idToolbarSpacer"/>
				<Button text="{i18n>Create}" type="Accept" class="btn-proceed" press="onCreate"/>
			</OverflowToolbar>
		</f:footer>
	</f:DynamicPage>
</mvc:View>