<core:FragmentDefinition xmlns="sap.ui.comp.valuehelpdialog" xmlns:m="sap.m" xmlns:fb="sap.ui.comp.filterbar" xmlns:core="sap.ui.core">
	<ValueHelpDialog title="Plant Details" ok=".onValueHelpOkPress" cancel=".onValueHelpCancelPress" afterClose=".onValueHelpAfterClose"
		afterOpen=".onValueHelpAfterOpen" supportRanges="false" key="Status" descriptionKey="Text" class="value-header">
		<filterBar>
			<fb:FilterBar showClearOnFB="true" advancedMode="true" search="onFilterBarSearch">
				<fb:filterGroupItems>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="Status" label="Status" visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Status"/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="Text" label="Text" visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Text"/>
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>