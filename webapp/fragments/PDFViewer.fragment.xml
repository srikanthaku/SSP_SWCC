<core:FragmentDefinition xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified"
	xmlns:f="sap.ui.layout.form">
	<Dialog contentWidth="60%" contentHeight="100%" afterClose="onCloseSlaAgreementDialog">
		<ScrollContainer id="myScrollContainer" height="100%" width="100%" vertical="true">
			<!--<core:HTML id="pdfContainer" content="{/pdf}"/>-->
			<core:HTML id="myHTMLControl" content=' &lt;embed type="application/pdf" src="{/pdf}" width="100%" height="600px" /&gt; '/>
		</ScrollContainer>
		<beginButton>
			<Button enabled="false" text="{i18n>Accept}" visible="{objectViewModel>/popupEditable}" press="onacceptpdf" type="Emphasized"/>
		</beginButton>
		<endButton>
			<Button enabled="false" text="{i18n>Decline}" press="onrejectpdf"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>