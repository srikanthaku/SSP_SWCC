<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:u="sap.ui.unified" xmlns:tnt="sap.tnt"
	xmlns:fm="sap.ui.layout.form" xmlns:upload="sap.m.upload" xmlns:core="sap.ui.core">
	<VBox width="100%">
		<fm:SimpleForm layout="ColumnLayout" class="ssp-form-bg" columnsM="2" columnsL="2" columnsXL="4" singleContainerFullSize="true"
			labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:toolbar>
				<Toolbar class="ssp-title">
					<Button icon="sap-icon://nav-back" press="handleBackPress"/>
					<Title class="ssp-form-bg" text="{i18n>createservicerequest}"/>
					<ToolbarSpacer/>
				</Toolbar>
			</fm:toolbar>
			<fm:content>
				<Toolbar>
					<Title level="H3" text="{/ServiceDescription/}" class="ssp-tlbr-title"/>
					<ToolbarSpacer/>
					<!--<Button icon="sap-icon://settings" press=".handleButtonPress"/>-->
				</Toolbar>
				<Label text="{i18n>Serviceproduct}" required="true"/>
				<Input value="{/SCMAppVisible/}" showSuggestion="" enabled="false" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>Plant}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_COMMON_SRV-/A_Plant-com.swcc.Template.fragments.CommonF4.PlantF4-/PlantF4/" width="100%"
					placeholder="Enter Plant" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/PlantF4/}">
					<customData>
						<core:CustomData key="Plant" value="Plant"/>
						<core:CustomData key="PlantName" value="Plant Name"/>
					</customData>
				</Input>
				<Label text="{i18n>quantity}" required="true"/>
				<Input placeholder="Enter Quantity" value=""/>
				<Label text="{i18n>crtype}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_SCM_SRV-/CRTypeSet-com.swcc.Template.fragments.SCMModule.CrtypeF4-/CrtypeF4/" width="100%"
					placeholder="Enter CR Type" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/CrtypeF4/}">
					<customData>
						<core:CustomData key="Key" value="CR Type"/>
						<core:CustomData key="Value" value="CR Name"/>
					</customData>
				</Input>
				<!--	<ComboBox width="100%" change="" selectionChange="onSelectPlant" valueState="None" placeholder="Select Plant"
					selectedKey="{/ClasssificationandInventory/ChangeRequest/Key}" items="{/CRf4/}">
					<core:Item key="{Key}" text="{Key} - {Value}"/>
				</ComboBox>-->
				<Label text="{i18n>Materialtype}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_SCM_SRV-/MaterialTypeSet-com.swcc.Template.fragments.SCMModule.MaterialtypeF4-/MaterialtypeF4/" width="100%"
					placeholder="Enter Material Type" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/MaterialtypeF4/}">
					<customData>
						<core:CustomData key="Key" value="Material Type"/>
						<core:CustomData key="Value" value="Material Name"/>
					</customData>
				</Input>
				<Label text="{i18n>Materialgroup}" required="false"/>
				<Input ariaLabelledBy="-ZSSP_SCM_SRV-/I_ExtProdGrp-com.swcc.Template.fragments.SCMModule.MaterialgroupF4-/MaterialgroupF4/" width="100%"
					placeholder="Enter Material group" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/MaterialgroupF4/}">
					<customData>
						<core:CustomData key="ExternalProductGroup" value="Material group"/>
						<core:CustomData key="ExternalProductGroup_Text" value="Material group name"/>
					</customData>
				</Input>
				<Label text="{i18n>Description}" required="true"/>
				<Input placeholder="Enter Description" value="{/ServiceDescription/}" enabled="false"/>
				<!--	<Label text="{i18n>workcenter}" required="true"/>
				<ComboBox width="100%" valueState="None" placeholder="Select Work Center" selectedKey="{workCenterModel>Workcenter}"
					items="{workCenterModel>/}">
					<core:Item key="{workCenterModel>Workcenter}" text="{workCenterModel>Workcenter} - {workCenterModel>WorkcenterDesc}"/>
				</ComboBox>-->
				<Label text="{i18n>Industrysector}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_SCM_SRV-/IndustrySectorSet-com.swcc.Template.fragments.SCMModule.IndustrysectorF4-/IndustrysectorF4/"
					width="100%" placeholder="Enter Industry Sector" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/IndustrysectorF4/}">
					<customData>
						<core:CustomData key="Key" value="Industry Sector"/>
						<core:CustomData key="Value" value="Industry Sector Name"/>
					</customData>
				</Input>
				<!--	<ComboBox width="100%" change="" selectionChange="onSelectPlant" id="industrySector" valueState="None" placeholder="Industry Sector"
					selectedKey="{/ClasssificationandInventory/ChangeRequest/Key}" items="{/IndustrialSectorf4/}">
					<core:Item key="{Key}" text="{Key} - {Value}"/>
				</ComboBox>-->
				<!--<Input placeholder="" showSuggestion="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequest"
					suggestionItems="{/ProductCollection}">
					<suggestionItems>
						<core:Item text="{Name}"/>
					</suggestionItems>
				</Input>-->
				<Label text="{i18n>Description}" required="true"/>
				<Input placeholder="" showSuggestion="" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>Baseuom}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_SCM_SRV-/ZCDSV_SCM_PRODUCTVH-com.swcc.Template.fragments.SCMModule.BaseunitF4-/BaseunitF4/" width="100%"
					placeholder="Enter Industry Sector" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/BaseunitF4/}">
					<customData>
						<core:CustomData key="Product" value="Product"/>
						<core:CustomData key="BaseUnit" value="Base Unit"/>
					</customData>
				</Input>
				<Label text="{i18n>Externalmaterial}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_SCM_SRV-/I_ExtProdGrp-com.swcc.Template.fragments.SCMModule.MaterialgroupF4-/MaterialgroupF4/" width="100%"
					placeholder="Enter Material group" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/MaterialgroupF4/}">
					<customData>
						<core:CustomData key="ExternalProductGroup" value="Material group"/>
						<core:CustomData key="ExternalProductGroup_Text" value="Material group name"/>
					</customData>
				</Input>
				<Label text="{i18n>Materialdesc}" required="true"/>
				<Input placeholder="" showSuggestion="" showValueHelp="" valueHelpRequest=""/>
				<!--	<ComboBox width="100%" change="" selectionChange="onSelectPlant" id="externalMaterialGroup" valueState="None"
					placeholder="External Material Group" selectedKey="{crModel>ExternalProductGroup}" items="{crModel>/externalMaterialGroup}">
					<core:Item key="{crModel>ExternalProductGroup}" text="{crModel>ExternalProductGroup} - {crModel>ExternalProductGroup_Text}"/>
				</ComboBox>-->
			</fm:content>
		</fm:SimpleForm>
	</VBox>
	<VBox width="100%">
		<fm:SimpleForm title="" layout="ResponsiveGridLayout" class="ssp-form-txtarea" columnsM="2" columnsL="3" columnsXL="4"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content>
				<Label text="{i18n>Description}" required="false"/>
				<TextArea placeholder="" rows="5" cols="20" growing="true" growingMaxLines="7" height="6rem" width="100%"/>
			</fm:content>
		</fm:SimpleForm>
	</VBox>
	<VBox class="ssp-table-box">
		<Toolbar>
			<Title level="H3" text="{i18n>Details}" class="ssp-tlbr-title"/>
			<ToolbarSpacer/>
			<!--<Button icon="sap-icon://settings" press=".handleButtonPress"/>-->
		</Toolbar>
		<fm:SimpleForm layout="ColumnLayout" class="ssp-form-bg" columnsM="2" columnsL="3" columnsXL="3" singleContainerFullSize="true"
			labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content>
				<Label text="{i18n>class}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_SCM_SRV-/ZCDSV_SCM_CLASS-com.swcc.Template.fragments.SCMModule.ClassF4-/ClassF4/" width="100%"
					placeholder="Enter Class" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/ClassF4/}">
					<customData>
						<core:CustomData key="Class" value="Class"/>
						<core:CustomData key="conv_char" value="Text"/>
					</customData>
				</Input>
				<!--<Input id="classdescription" placeholder="" showSuggestion="" showValueHelp="" valueHelpRequest=""/>-->
			</fm:content>
		</fm:SimpleForm>
		<Table id="totalamount2" sticky="ColumnHeaders,HeaderToolbar" inset="false" class="ssp-request-Table sapUiContentPadding">
			<columns>
				<Column>
					<Text text="Sno"/>
				</Column>
				<Column width="">
					<Text text="{i18n>Characteristicsdescription}"/>
				</Column>
				<Column minScreenWidth="Tablet" demandPopin="true">
					<Text text="{i18n>value}"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem vAlign="Middle">
					<cells>
						<Text text="1"/>
						<Text text="Titanium"/>
						<Text text="Will shi"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</VBox>
	<VBox class="uploadBox sapUiSmallMarginBeginEnd">
		<Label text="{i18n>Attachments}" required="false"/>
		<u:FileUploader class="sapUiContentPadding" buttonOnly="true" multiple="true" icon="sap-icon://attachment" visible="true"
			sameFilenameAllowed="false" buttonText="Upload file" change="onFileAdded" filenameLengthExceed="onFileNameLengthExceed"
			fileSizeExceed="onFileSizeExceed" maximumFileSize="2" uploadComplete="onFileComplete1" typeMissmatch="handleMissmatch"
			placeholder="Choose a file" style="Emphasized"
			mimeType="application/msword,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document"/>
	</VBox>
	<VBox>
		<Table items="{/PMCreateRequest/UploadedData}" class="upload-table" mode="Delete" noDataText="{i18n>uploadtooltip}">
			<headerToolbar >
				<Toolbar>
					<content>
						<Image src="../webapp/util/Images/uploadfile.png" height="auto" class="hide-data" width="auto">
							<layoutData>
								<FlexItemData growFactor="1"/>
							</layoutData>
						</Image>
					</content>
				</Toolbar>
			</headerToolbar>
			<columns>
				<Column>
					<Text text="File Name"/>
				</Column>
				<Column>
					<Text text="File Size"/>
				</Column>
				<Column>
					<Text text="Delete"/>
				</Column>
				<!-- Add more columns as needed -->
			</columns>
			<items>
				<ColumnListItem>
					<cells>
						<Text text="{Filename}"/>
						<Text text="{Filesize}"/>
						<Text textAlign="Center" text="{Delete}"/>
						<!-- Add more cells as needed -->
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</VBox>
</core:FragmentDefinition>