<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns:u="sap.ui.unified" xmlns="sap.m" xmlns:tnt="sap.tnt"
	xmlns:fm="sap.ui.layout.form" xmlns:upload="sap.m.upload" xmlns:core="sap.ui.core">
	<VBox width="100%">
		<fm:SimpleForm layout="ResponsiveGridLayout" class="ssp-form-bg" columnsM="2" columnsL="2" columnsXL="2" singleContainerFullSize="true"
			labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:toolbar>
				<Toolbar class="ssp-title">
					<Button icon="sap-icon://nav-back" press="handleBackPress"/>
					<Title class="ssp-form-bg" text="{i18n>createservicerequest}"/>
					<ToolbarSpacer/>
				</Toolbar>
			</fm:toolbar>
			<fm:content>
				<Toolbar>
					<Title level="H3" text="{/ServiceDescription/}" class="ssp-tlbr-title"/>
					<ToolbarSpacer/>
					<!--<Button icon="sap-icon://settings" press=".handleButtonPress"/>-->
				</Toolbar>
				<Label text="{i18n>serviceproduct}" required="true"/>
				<Input value="{/HRAppVisible/}" showSuggestion="" enabled="false" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>plant}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_COMMON_SRV-/A_Plant-com.swcc.Template.fragments.CommonF4.PlantF4-/PlantF4/" width="100%"
					placeholder="Enter Plant" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/PlantF4/}">
					<customData>
						<core:CustomData key="Plant" value="Plant"/>
						<core:CustomData key="PlantName" value="Plant Name"/>
					</customData>
				</Input>
				<Label text="{i18n>userid}" required="false"/>
				<Input enabled="false" value="{/LoginUserID}" valueHelpRequest=""/>
				<Label text="{i18n>name}" required="true"/>
				<Input placeholder="Enter Name" value="{/BenefitsManagement/ClaimRequest/Header/PadCname/}" showSuggestion="" showValueHelp=""
					valueHelpRequest=""/>
				<Label text="{i18n>mobileNumber}" required="true"/>
				<Input placeholder="Enter Mobile Number" type="Number" value="{/BenefitsManagement/ClaimRequest/Header/Zmobile/}" showSuggestion=""
					showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>insurancepolicy}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_HR_SRV-/ZCDSV_SF_INSPOLICY-com.swcc.Template.fragments.HRModule.InsurancepolicyF4-/InsurancepolicyF4/"
					width="100%" placeholder="Select Insurance Policy" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/InsurancepolicyF4/}">
					<customData>
						<core:CustomData key="keyfield" value="Insurance policy"/>
						<core:CustomData key="value_1" value="Insurance policy type"/>
					</customData>
				</Input>
				<Label text="{i18n>amount}" required="true"/>
				<Input placeholder="Enter Amount" value="{/BenefitsManagement/ClaimRequest/Header/Zamount/}"/>
				<Label text="{i18n>bankname}" required="true"/>
				<Input placeholder="Enter Bank Name" value="{/BenefitsManagement/ClaimRequest/Header/Bankk/}"/>
				<Label text="{i18n>country}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_USER_SRV-/A_CountryText-com.swcc.Template.fragments.HRModule.CountryF4-/CountryF4/" width="100%"
					placeholder="Select Country" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/CountryF4/}">
					<customData>
						<core:CustomData key="Country" value="Country"/>
						<core:CustomData key="CountryName" value="CountryName"/>
					</customData>
				</Input>
				<Label text="{i18n>providername}" required="true"/>
				<Input value="{/BenefitsManagement/ClaimRequest/Header/Zproname/}" placeholder="Enter Provider Name" showSuggestion="" showValueHelp=""
					valueHelpRequest=""/>
				<core:Title text=""/>
				<Label text="{i18n>Description}" required="true"/>
				<Input placeholder="Enter Description" value="{/ServiceDescription/}" enabled="false"/>
				<Label text="{i18n>whenshould}" required="true"/>
				<DatePicker valueFormat="dd/MM/yyyy" displayFormat="dd/MM/yyyy" value="{/BenefitsManagement/ClaimRequest/Header/Begda/}"
					change="handleChange" class=""/>
				<Label text="{i18n>membershipnumber}" required="true"/>
				<Input placeholder="Enter Membership Number" value="{/BenefitsManagement/ClaimRequest/Header/Zmemnum/}" valueHelpRequest=""/>
				<Label text="{i18n>National}" required="true"/>
				<Input value="{/BenefitsManagement/ClaimRequest/Header/P25Idcot/}" placeholder="Enter National-ID" valueHelpRequest=""/>
				<Label text="{i18n>iban}" required="true"/>
				<Input value="{/BenefitsManagement/ClaimRequest/Header/Iban/}" placeholder="Enter IBAN" valueHelpRequest=""/>
				<Label text="{i18n>gender}" required="true"/>
				<ComboBox selectedKey="{/BenefitsManagement/ClaimRequest/Header/Gesch/}" placeholder="Select Gender" width="100%" valueState="None">
					<items>
						<core:Item key="M" text="Male"/>
						<core:Item key="F" text="Female"/>
					</items>
				</ComboBox>
				<Label text="{i18n>City}" required="true"/>
				<Input placeholder="Enter City" value="{/BenefitsManagement/ClaimRequest/Header/PadOrt01/}"/>
				<Label text="{i18n>visitdate}" required="true"/>
				<DatePicker valueFormat="dd/MM/yyyy" value="{/BenefitsManagement/ClaimRequest/Header/Zvisdat/}" change="handleChange" class=""/>
				<Label text="{i18n>claimtype}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_HR_SRV-/ZCDSV_SF_CLAIMTYPE-com.swcc.Template.fragments.HRModule.ClaimtypeF4-/ClaimtypeF4/" width="100%"
					placeholder="Select Claim type" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/ClaimtypeF4/}">
					<customData>
						<core:CustomData key="keyfield" value="Claim"/>
						<core:CustomData key="value_1" value="Claim type"/>
					</customData>
				</Input>
				<Label text=""/>
				<Link text="{i18n>claimrequest}" class="sapUiMediumMarginTop" target="_blank" emphasized="true"
					href="ui5_ui5/sap/zssp_app/util/SLA_PDF/SLA.pdf"/>
			</fm:content>
		</fm:SimpleForm>
	</VBox>
	<VBox width="100%">
		<fm:SimpleForm title="" layout="ResponsiveGridLayout" class="ssp-form-txtarea" columnsM="2" columnsL="3" columnsXL="4"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content>
				<Label text="{i18n>comments}" required="false"/>
				<TextArea placeholder="" rows="5" cols="20" value="{/BenefitsManagement/ClaimRequest/Header/Zcomment/}" growing="true" growingMaxLines="7"
					height="6rem" width="100%"/>
			</fm:content>
		</fm:SimpleForm>
	</VBox>
	<VBox>
		<Table items="{/UploadedData}" class="upload-data" noDataText="{i18n>medicalinsuranceattachement}">
			<headerToolbar>
				<Toolbar>
					<content>
						<FlexBox class="sapUiTinyMarginTop" alignItems="Start" justifyContent="SpaceBetween">
							<Title text="{i18n>Attachments}" level="H1"></Title>
							<Text text="{i18n>uploadtooltip}" class="sapUiTinyMarginBegin upload-info">
								<layoutData>
									<FlexItemData growFactor="1"/>
								</layoutData>
							</Text>
							<u:FileUploader class="sapUiContentPadding" buttonOnly="true" multiple="true" icon="sap-icon://attachment" visible="true"
								sameFilenameAllowed="false" buttonText="Upload file" change="onFileAdded" filenameLengthExceed="onFileNameLengthExceed"
								fileSizeExceed="onFileSizeExceed" maximumFileSize="2" uploadComplete="onFileComplete1" typeMissmatch="handleMissmatch"
								placeholder="Choose a file" style="Emphasized"
								mimeType="application/msword,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document"/>
						</FlexBox>
					</content>
				</Toolbar>
			</headerToolbar>
			<columns>
				<Column>
					<Text text="File Name"/>
				</Column>
				<Column>
					<Text text="File Size"/>
				</Column>
				<Column>
					<Text text="Delete"/>
				</Column>
				<!-- Add more columns as needed -->
			</columns>
			<items>
				<ColumnListItem>
					<cells>
						<Text text="{Filename}"/>
						<Text text="{path:'Filesize',formatter:'.formatBytesToMB'}"/>
						<Button type="Reject" text="Delete" press="onDeleteAttachment"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</VBox>
</core:FragmentDefinition>