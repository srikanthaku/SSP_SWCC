<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:tnt="sap.tnt" xmlns:fm="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox width="100%">
		<fm:SimpleForm title="{i18n>createservicerequest}" layout="ColumnLayout" class="ssp-form-bg" columnsM="2" columnsL="3" columnsXL="4"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:toolbar>
				<Toolbar class="ssp-title">
					<Button press=".handleBackPress" icon="sap-icon://nav-back"/>
					<Title class="ssp-form-bg" text="{i18n>createservicerequest}"/>
					<ToolbarSpacer/>
				</Toolbar>
			</fm:toolbar>
			<fm:content>
				<Toolbar>
					<Title level="H3" text="{/ServiceDescription/}" class="ssp-tlbr-title"/>
					<ToolbarSpacer/>
				</Toolbar>
				<Label text="{i18n>serviceproduct}"/>
				<Input value="{/FinanceAppVisible/}" showSuggestion="" enabled="false" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>quantity}" required="true"/>
				<Input placeholder="Enter Quantity" value="{/AccountsReceivable/Manageandprocess/Header/quantity/}"/>
				<Label text="{i18n>plant}" required="true"/>
				<ComboBox width="100%" change="onCheckPlantVal" selectionChange="onSelectPlant" valueState="None" placeholder="Select Plant"
					selectedKey="{/AccountsReceivable/Manageandprocess/Header/Plant/}" items="{/PlantF4/}">
					<core:Item key="{Plant}" text="{Plant} - {PlantName}"/>
				</ComboBox>
				<!--	<Label text="{i18n>workcenter}" required="true"/>
				<ComboBox width="100%" valueState="None" placeholder="Select Work Center" selectedKey="{fiModel>workCenter}" items="{fiModel>/workCenter}">
					<core:Item key="{fiModel>Workcenter}" text="{fiModel>Workcenter} - {fiModel>WorkcenterDesc}"/>
				</ComboBox>-->
				<Label text="{i18n>Description}" required="false"/>
				<Input placeholder="Enter Description" value="{/ServiceDescription/}" enabled="false"/>
				<Label text="{i18n>shortdescription}" required="true"/>
				<Input placeholder="Allowed 40 Charcters" showSuggestion="" showValueHelp="" value="{/PMCreateRequest/Header/Descript/}" valueHelpRequest=""/>
				<Label text="{i18n>CompanyCode}" required="true"/>
				<!--<Input required="true" width="100%" value="1000"/>-->
				<ComboBox width="100%" change="" selectionChange="" valueState="None" placeholder="Company Code"
					selectedKey="{/AccountsReceivable/Manageandprocess/Header/CompanyCode}" items="{/CompanyF4/}">
					<core:Item key="{CompanyCode}" text="{CompanyCode} - {CompanyCodeName}"/>
				</ComboBox>
				<Label text="{i18n>BANKGL}" required="true"/>
				<!--<ComboBox width="100%" change="" selectionChange="" valueState="None" placeholder="Select GL Account"-->
				<!--	selectedKey="{/AccountsReceivable/Manageandprocess/Header/GLAccount/}" items="{/GeneralLedgerf4/}">-->
				<!--	<core:Item key="{CompanyCode}" text="{GLAccount} - {GLAccountName}"/>-->
				<!--</ComboBox>-->
				<Input ariaLabelledBy="-ZSSP_FI_SRV-/I_GeneralLedgerAccountVH-com.swcc.Template.fragments.FinanceModule.GlaccountF4" width="100%"
					placeholder="Enter GL Account" valueHelpOnly="true" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/GLAccount}">
					<customData>
						<core:CustomData key="GLAccount" value="GLAccount"/>
						<core:CustomData key="GLAccountName" value="GLAccountName"/>
					</customData>
				</Input>
				<Label text="{i18n>CustomerCode}" required="true"/>
				<ComboBox width="100%" change="" selectionChange="" id="customercode" valueState="None" placeholder="Customer Code"
					selectedKey="{/AccountsReceivable/Manageandprocess/Header/Customer}" items="{/CustomerCodef4/}">
					<core:Item key="{Customer}" text="{Customer} - {CustomerName}"/>
				</ComboBox>
				<Label text="{i18n>Postingdt}" required="true"/>
				<DateRangeSelection showCurrentDateButton="true" showFooter="true" change="handleChange"
					value="{ 'type': 'sap.ui.model.type.DateInterval', 'parts': [ { 'type': 'sap.ui.model.type.Date', 'path': '/oDRS3Start' }, { 'type': 'sap.ui.model.type.Date', 'path': '/oDRS3End' } ] }"/>
			</fm:content>
			<Button text="Search" type="Default" press="onSearchMngePttyCash" width="40%" class="sapUiTinyMarginBegin sapUiContentPadding swcc-pmy-btn"/>
		</fm:SimpleForm>
	</VBox>
	<VBox width="100%">
		<fm:SimpleForm title="" layout="ResponsiveGridLayout" class="ssp-form-txtarea" columnsM="2" columnsL="3" columnsXL="4"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content>
				<Label text="{i18n>Remarks}" required="false"/>
				<TextArea placeholder="" rows="5" cols="20" text="{/PMCreateRequest/Header/NotifText/}" growing="true" growingMaxLines="7" height="6rem"
					width="100%"/>
			</fm:content>
		</fm:SimpleForm>
	</VBox>
	<VBox class="ssp-table-box">
		<!--<fm:SimpleForm id="ssp-create-sr-2" layout="ColumnLayout" class="ssp-form-bg" columnsM="2" columnsL="3" columnsXL="3"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content>-->
		<Toolbar>
			<Title level="H3" text="{i18n>Details}" class="ssp-tlbr-title"/>
			<ToolbarSpacer/>
		</Toolbar>
		<ScrollContainer horizontal="true" vertical="false" height="auto">
			<Table mode="None" items="{path : '/MarineTransportation/itemData'}" sticky="ColumnHeaders,HeaderToolbar"
				class="ssp-request-Table sapUiContentPadding" inset="false" growingThreshold="5" growingScrollToLoad="true" growing="true"
				fixedLayout="true">
				<columns>
					<Column>
						<Text text="Invoice Number"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Posting Date"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Reference"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Sales Order Number"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Item Number"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Amount"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Currency"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Customer Code"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Material Code"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Quantity"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem vAlign="Middle">
						<cells>
							<Text text="2"/>
							<Text text="2"/>
							<Text text="Will shi"/>
							<Text text="2"/>
							<Input value="{BaseUnit}"/>
							<Text text="2"/>
							<Text text="2"/>
							<Text text="2"/>
							<Text text="2"/>
							<Text text="2"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</ScrollContainer>
		<!--	</fm:content>
		</fm:SimpleForm>-->
	</VBox>
</core:FragmentDefinition>