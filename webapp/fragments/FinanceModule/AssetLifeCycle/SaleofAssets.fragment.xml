<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns:u="sap.ui.unified" xmlns="sap.m" xmlns:tnt="sap.tnt"
	xmlns:fm="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<VBox width="100%">
		<fm:SimpleForm layout="ColumnLayout" class="ssp-form-bg" columnsM="2" columnsL="3" columnsXL="4" singleContainerFullSize="true"
			labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:toolbar>
				<Toolbar class="ssp-title">
					<Button press=".handleBackPress" icon="sap-icon://nav-back"/>
					<Title class="ssp-form-bg" text="{i18n>createservicerequest}"/>
					<ToolbarSpacer/>
				</Toolbar>
			</fm:toolbar>
			<fm:content>
				<Toolbar>
					<Title level="H3" text="{/ServiceDescription/}" class="ssp-tlbr-title"/>
					<ToolbarSpacer/>
				</Toolbar>
				<Label text="{i18n>serviceproduct}"/>
				<Input value="{/FinanceAppVisible/}" showSuggestion="" enabled="false" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>quantity}" required="true"/>
				<Input placeholder="Enter Quantity" value="{/AssetLifecycle/SaleofAssets/Header/quantity/}"/>
				<Label text="{i18n>CompanyCode}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_COMMON_SRV-/C_CompanyCodeVHTemp-com.swcc.Template.fragments.CommonF4.CompanycodeF4-/CompanycodeF4/"
					width="100%" placeholder="Enter Company code" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/CompanycodeF4/}">
					<customData>
						<core:CustomData key="CompanyCode" value="Company Code"/>
						<core:CustomData key="CompanyCodeName" value="Company Name"/>
					</customData>
				</Input>
				<!--	<Label text="{i18n>workcenter}" required="true"/>
				<ComboBox width="100%" valueState="None" placeholder="Select Work Center" selectedKey="{/PMCreateRequest/Header/PmWkctr/}"
					items="{/PMCreateRequest/WorkCenterF4/}">
					<core:Item key="{Workcenter}" text="{Workcenter} - {WorkcenterDesc}"/>
				</ComboBox>-->
				<Label text="{i18n>shortdescription}" required="true"/>
				<Input placeholder="Allowed 40 Charcters" showSuggestion="" showValueHelp="" value="{/AssetLifecycle/SaleofAssets/Header/Descript/}"
					valueHelpRequest=""/>
				<Label text="{i18n>AssetNumber}" required="true"/>
				<Input placeholder="Enter Asset Number" value="{/AssetLifecycle/SaleofAssets/Header/Anln1/}" required="true" width="100%"/>
				<Label text="{i18n>Description}" required="false"/>
				<Input placeholder="Enter Description" value="{/ServiceDescription/}" enabled="false"/>
				<Label text="{i18n>AccountingPrinciple}" required="true"/>
				<Input
					ariaLabelledBy="-ZSSP_FI_SRV-/I_AccountingPrincipleText-com.swcc.Template.fragments.FinanceModule.AccountingprincipalF4-/AccountingprincipalF4/"
					width="100%" placeholder="Enter Accounting Principal" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/AccountingprincipalF4/}">
					<customData>
						<core:CustomData key="AccountingPrinciple" value="Accounting Principle"/>
						<core:CustomData key="AccountingPrincipleName" value="Accounting Principle Name"/>
					</customData>
				</Input>
				<Label text="{i18n>DepreciationArea}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_FI_SRV-/I_DeprAreaForLedgerVH-com.swcc.Template.fragments.FinanceModule.DepreciationF4-/DepreciationF4/"
					width="100%" placeholder="Enter Area" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/DepreciationF4/}">
					<customData>
						<core:CustomData key="AssetDepreciationArea" value="Asset Depreciation"/>
						<core:CustomData key="AssetDepreciationArea_Text" value="Asset Depreciation Name"/>
					</customData>
				</Input>
				<Label text="{i18n>DocumentDate}" required="true"/>
				<DatePicker valueFormat="dd/MM/yyyy" value="{/AssetLifecycle/SaleofAssets/Header/Bldat/}" change="handleChange" class=""/>
				<Label text="{i18n>PostingDate}" required="true"/>
				<DatePicker valueFormat="dd/MM/yyyy" value="{/AssetLifecycle/SaleofAssets/Header/Budat/}" change="handleChange" class=""/>
				<Label text="{i18n>plant}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_COMMON_SRV-/A_Plant-com.swcc.Template.fragments.CommonF4.PlantF4-/PlantF4/" width="100%"
					placeholder="Enter Plant" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/PlantF4/}">
					<customData>
						<core:CustomData key="Plant" value="Plant"/>
						<core:CustomData key="PlantName" value="Plant Name"/>
					</customData>
				</Input>
				<Label text="{i18n>AssetValueDate}" required="true"/>
				<DatePicker valueFormat="dd/MM/yyyy" value="{/AssetLifecycle/SaleofAssets/Header/Bzdat/}" change="handleChange" class=""/>
				<Label text="{i18n>Revenue}" required="true"/>
				<Input type="Number" value="{/AssetLifecycle/SaleofAssets/Header/Erlbt/}" placeholder="Enter Revenue" required="true" width="100%"/>
			</fm:content>
		</fm:SimpleForm>
	</VBox>
	<VBox width="100%">
		<fm:SimpleForm title="" layout="ResponsiveGridLayout" class="ssp-form-txtarea" columnsM="2" columnsL="3" columnsXL="4"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content>
				<Label text="{i18n>Remarks}" required="false"/>
				<TextArea placeholder="" rows="5" cols="20" value="{/AssetLifecycle/SaleofAssets/Header/NotifText/}" growing="true" growingMaxLines="7"
					height="6rem" width="100%"/>
			</fm:content>
		</fm:SimpleForm>
	</VBox>
	<VBox class="uploadBox sapUiSmallMarginBeginEnd">
		<Label text="{i18n>Attachments}" required="false"/>
		<u:FileUploader class="sapUiContentPadding" buttonOnly="true" multiple="true" icon="sap-icon://attachment" visible="true"
			sameFilenameAllowed="false" buttonText="Upload file" change="onFileAdded" filenameLengthExceed="onFileNameLengthExceed"
			fileSizeExceed="onFileSizeExceed" maximumFileSize="2" uploadComplete="onFileComplete1" typeMissmatch="handleMissmatch"
			placeholder="Choose a file" style="Emphasized"
			mimeType="application/msword,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document"/>
	</VBox>
	<VBox>
		<Table items="{/UploadedData}" class="upload-table" mode="Delete" noDataText="{i18n>uploadtooltip}">
			<headerToolbar >
				<Toolbar>
					<content>
						<Image src="../webapp/util/Images/uploadfile.png" height="auto" class="hide-data" width="auto">
							<layoutData>
								<FlexItemData growFactor="1"/>
							</layoutData>
						</Image>
					</content>
				</Toolbar>
			</headerToolbar>
			<columns>
				<Column>
					<Text text="File Name"/>
				</Column>
				<Column>
					<Text text="File Size"/>
				</Column>
				<Column>
					<Text text="Delete"/>
				</Column>
				<!-- Add more columns as needed -->
			</columns>
			<items>
				<ColumnListItem>
					<cells>
						<Text text="{Filename}"/>
						<Text text="{Filesize}"/>
						<Text textAlign="Center" text="{Delete}"/>
						<!-- Add more cells as needed -->
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</VBox>
</core:FragmentDefinition>