<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns:u="sap.ui.unified" xmlns="sap.m" xmlns:tnt="sap.tnt"
	xmlns:fm="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<VBox width="100%">
		<fm:SimpleForm layout="ColumnLayout" class="ssp-form-bg" columnsM="2" columnsL="3" columnsXL="4" singleContainerFullSize="true"
			labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:toolbar>
				<Toolbar class="ssp-title">
					<Button press=".handleBackPress" icon="sap-icon://nav-back"/>
					<Title class="ssp-form-bg" text="{i18n>createservicerequest}"/>
					<ToolbarSpacer/>
				</Toolbar>
			</fm:toolbar>
			<fm:content>
				<Toolbar>
					<Title level="H3" text="{/ServiceDescription/}" class="ssp-tlbr-title"/>
					<ToolbarSpacer/>
				</Toolbar>
				<Label text="{i18n>serviceproduct}"/>
				<Input value="{/FinanceAppVisible/}" showSuggestion="" enabled="false" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>quantity}" required="true"/>
				<Input placeholder="Enter Quantity" value="{/AssetLifecycle/RecordAsset/Header/quantity/}"/>
				<!--	<Label text="{i18n>workcenter}" required="true"/>
				<ComboBox width="100%" valueState="None" placeholder="Select Work Center" selectedKey="{/PMCreateRequest/Header/PmWkctr/}"
					items="{/PMCreateRequest/WorkCenterF4/}">
					<core:Item key="{Workcenter}" text="{Workcenter} - {WorkcenterDesc}"/>
				</ComboBox>-->
				<Label text="{i18n>shortdescription}" required="true"/>
				<Input placeholder="Allowed 40 Charcters" showSuggestion="" showValueHelp="" value="{/PMCreateRequest/Header/Descript/}" valueHelpRequest=""/>
				<Label text="{i18n>CompanyCode}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_COMMON_SRV-/C_CompanyCodeVHTemp-com.swcc.Template.fragments.CommonF4.CompanycodeF4-/CompanycodeF4/"
					width="100%" placeholder="Enter Company code" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/CompanycodeF4/}">
					<customData>
						<core:CustomData key="CompanyCode" value="Company Code"/>
						<core:CustomData key="CompanyCodeName" value="Company Name"/>
					</customData>
				</Input>
				<Label text="{i18n>AssetClass}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_FI_SRV-/I_AssetClassStdVH-com.swcc.Template.fragments.FinanceModule.Assestclass-/AssestclassF4/" width="100%"
					placeholder="Enter Assest Class" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/AssestclassF4/}">
					<customData>
						<core:CustomData key="AssetClass" value="Asset Class"/>
						<core:CustomData key="AssetClass_Text" value="Asset Class Text"/>
					</customData>
				</Input>
				<Label text="{i18n>SubNumber}" required="true"/>
				<ComboBox width="100%" change="onCheckPlantVal" selectionChange="onSelectPlant" valueState="None" placeholder="{i18n>AssetClass}"
					selectedKey="{/AssetLifecycle/RecordAsset/Header/AssetClass}" items="{/AssestSubf4/}">
					<core:Item key="{AssetClass}" text="{AssetClass} - {AssetClass_Text}"/>
				</ComboBox>
				<Label text="{i18n>Description}" required="true"/>
				<Input required="true" width="100%"/>
				<Label text="{i18n>Description}" required="false"/>
				<Input placeholder="Enter Description" value="{/ServiceDescription/}" enabled="false"/>
				<Label text="{i18n>AssetMainNoText}" required="true"/>
				<Input required="true" width="100%"/>
				<Label text="{i18n>InventoryNumber}" required="true"/>
				<Input required="true" width="100%"/>
				<Label text="{i18n>InventoryNote}" required="true"/>
				<Input required="true" width="100%"/>
				<Label text="{i18n>CaptitalizedOn}" required="true"/>
				<Input required="true" width="100%"/>
				<Label text="{i18n>CostCenter}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_FI_SRV-/I_CostCenterDetail-com.swcc.Template.fragments.FinanceModule.CostcenterF4-/costF4/" width="100%"
					placeholder="Enter Area" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/costF4/}">
					<customData>
						<core:CustomData key="ControllingArea" value="Cost Center"/>
						<core:CustomData key="CostCenter" value="Cost Text"/>
					</customData>
				</Input>
				<Label text="{i18n>ProfitCenter}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_FI_SRV-/I_CostCenterDetail-com.swcc.Template.fragments.FinanceModule.CostcenterF4-/costF4/" width="100%"
					placeholder="Enter Area" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/costF4/}">
					<customData>
						<core:CustomData key="ControllingArea" value="Cost Center"/>
						<core:CustomData key="CostCenter" value="Cost Text"/>
					</customData>
				</Input>
				<Label text="{i18n>plant}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_COMMON_SRV-/A_Plant-com.swcc.Template.fragments.CommonF4.PlantF4-/PlantF4/" width="100%"
					placeholder="Enter Plant" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/PlantF4/}">
					<customData>
						<core:CustomData key="Plant" value="Plant"/>
						<core:CustomData key="PlantName" value="Plant Name"/>
					</customData>
				</Input>
				<Label text="{i18n>Area}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_FI_SRV-/I_Group1AssetEvaluationKey-com.swcc.Template.fragments.FinanceModule.AreaF4-/AreaF4/" width="100%"
					placeholder="Enter Area" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/AreaF4/}">
					<customData>
						<core:CustomData key="Group1AssetEvaluationKey" value="Area"/>
						<core:CustomData key="Group1AssetEvaluationKey_Text" value="Area Text"/>
					</customData>
				</Input>
				<Label text="{i18n>Unit}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_FI_SRV-/I_Group2AssetEvaluationKey-com.swcc.Template.fragments.FinanceModule.UnitF4-/UnitF4/" width="100%"
					placeholder="Enter Unit" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/UnitF4/}">
					<customData>
						<core:CustomData key="Group2AssetEvaluationKey" value="Unit"/>
						<core:CustomData key="Group2AssetEvaluationKey_Text" value="Unit Text"/>
					</customData>
				</Input>
				<Label text="{i18n>System}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_FI_SRV-/I_Group3AssetEvaluationKey-com.swcc.Template.fragments.FinanceModule.SystemF4-/SystemF4/" width="100%"
					placeholder="Enter System" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/SystemF4/}">
					<customData>
						<core:CustomData key="Group3AssetEvaluationKey" value="System"/>
						<core:CustomData key="Group3AssetEvaluationKey_Text" value="System Text"/>
					</customData>
				</Input>
				<Label text="{i18n>AssetSuperNumber}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_FI_SRV-/ZCDS_ASSET_SUPER-com.swcc.Template.fragments.FinanceModule.Assestsupernumber-/AssestsupernumberF4/"
					width="100%" placeholder="Enter Plant" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/AssestsupernumberF4/}">
					<customData>
						<core:CustomData key="AssetSuperNum" value="Asset Super Num"/>
						<core:CustomData key="AssetSuperText" value="Asset Super Text"/>
					</customData>
				</Input>
				<!--	<ComboBox width="100%" change="onCheckPlantVal" selectionChange="onSelectPlant" valueState="None" placeholder="{i18n>AssetSuperNumber}"
					selectedKey="{/AssetLifecycle/RecordAsset/Header/AssetSuperNum}" items="{/AssestSuperf4/}">
					<core:Item key="{AssetSuperNum}" text="{AssetSuperNum} - {AssetSuperText}"/>
				</ComboBox>-->
				<Label text="{i18n>NonTechnicalAsset}" required="true"/>
				<Input
					ariaLabelledBy="-ZSSP_FI_SRV-/ZCDS_NONTECH_ASSET-com.swcc.Template.fragments.FinanceModule.AssetLifeCycle.NonTechAssetF4-/AssestnontechF4/"
					width="100%" placeholder="Enter Non Technical Asset" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/AssestnontechF4/}">
					<customData>
						<core:CustomData key="AssetNonTech" value="Asset Non Tech"/>
						<core:CustomData key="AssetNonTechText" value="Asset Non Tech Text"/>
					</customData>
				</Input>
			</fm:content>
		</fm:SimpleForm>
	</VBox>
	<VBox width="100%">
		<fm:SimpleForm title="" layout="ResponsiveGridLayout" class="ssp-form-txtarea" columnsM="2" columnsL="3" columnsXL="4"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content>
				<Label text="{i18n>Remarks}" required="false"/>
				<TextArea placeholder="" rows="5" cols="20" text="{/PMCreateRequest/Header/NotifText/}" growing="true" growingMaxLines="7" height="6rem"
					width="100%"/>
			</fm:content>
		</fm:SimpleForm>
	</VBox>
	<VBox class="uploadBox sapUiSmallMarginBeginEnd">
		<Label text="{i18n>Attachments}" required="false"/>
		<u:FileUploader class="sapUiContentPadding" buttonOnly="true" multiple="true" icon="sap-icon://attachment" visible="true"
			sameFilenameAllowed="false" buttonText="Upload file" change="onFileAdded" filenameLengthExceed="onFileNameLengthExceed"
			fileSizeExceed="onFileSizeExceed" maximumFileSize="2" uploadComplete="onFileComplete1" typeMissmatch="handleMissmatch"
			placeholder="Choose a file" style="Emphasized"
			mimeType="application/msword,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document"/>
	</VBox>
	<VBox>
		<Table items="{/PMCreateRequest/UploadedData}" class="upload-table" mode="Delete" noDataText="{i18n>uploadtooltip}">
			<headerToolbar >
				<Toolbar>
					<content>
						<Image src="../webapp/util/Images/uploadfile.png" height="auto" class="hide-data" width="auto">
							<layoutData>
								<FlexItemData growFactor="1"/>
							</layoutData>
						</Image>
					</content>
				</Toolbar>
			</headerToolbar>
			<columns>
				<Column>
					<Text text="File Name"/>
				</Column>
				<Column>
					<Text text="File Size"/>
				</Column>
				<Column>
					<Text text="Delete"/>
				</Column>
				<!-- Add more columns as needed -->
			</columns>
			<items>
				<ColumnListItem>
					<cells>
						<Text text="{Filename}"/>
						<Text text="{Filesize}"/>
						<Text textAlign="Center" text="{Delete}"/>
						<!-- Add more cells as needed -->
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</VBox>
</core:FragmentDefinition>