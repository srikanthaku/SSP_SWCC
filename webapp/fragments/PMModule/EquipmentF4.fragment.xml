<core:FragmentDefinition xmlns="sap.ui.comp.valuehelpdialog" xmlns:m="sap.m" xmlns:fb="sap.ui.comp.filterbar" xmlns:core="sap.ui.core">
	<ValueHelpDialog title="Equipment Details" ok=".onValueHelpOkPress" cancel=".onValueHelpCancelPress" afterClose=".onValueHelpAfterClose"
		afterOpen=".onValueHelpAfterOpen" supportRanges="false" key="Id" descriptionKey="Equipment">
		<filterBar>
			<fb:FilterBar advancedMode="true" search="onFilterBarSearch">
				<fb:filterGroupItems>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="Equipment" label="Equipment Code" visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Equipment"/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="EquipmentName" label="Equipment Name" visibleInFilterBar="true">
						<fb:control>
							<m:Input name="EquipmentName"/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="Object" label="Object Type" visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Object"/>
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>