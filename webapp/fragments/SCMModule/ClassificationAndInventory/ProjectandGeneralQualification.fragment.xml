<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:tnt="sap.tnt" xmlns:fm="sap.ui.layout.form"
	xmlns:upload="sap.m.upload" xmlns:core="sap.ui.core">
	<VBox width="100%">
		<fm:SimpleForm title="{i18n>createservicerequest}" layout="ColumnLayout" class="ssp-form-bg" columnsM="2" columnsL="2" columnsXL="4"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content>
				<Toolbar>
					<Title level="H3" text="{i18n>qualificationservice}" class="ssp-tlbr-title"/>
					<ToolbarSpacer/>
					<!--<Button icon="sap-icon://settings" press=".handleButtonPress"/>-->
				</Toolbar>
				<Label text="{i18n>Serviceproduct}" required="true"/>
				<Input placeholder="" showSuggestion="" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>Plant}" required="true"/>
				<Input placeholder="" showSuggestion="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequest"
					suggestionItems="{/ProductCollection}">
					<suggestionItems>
						<core:Item text="{Name}"/>
					</suggestionItems>
				</Input>
				<Label text="{i18n>typeofqualification}" required="true"/>
				<Input placeholder="" showSuggestion="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequest"
					suggestionItems="{/ProductCollection}">
					<suggestionItems>
						<core:Item text="{Name}"/>
					</suggestionItems>
				</Input>
				<Label text="{i18n>suppliername}" required="true"/>
				<Input placeholder="" showSuggestion="" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>suppliercontactno}" required="true"/>
				<Input placeholder="" showSuggestion="" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>projectname}" required="true"/>
				<Input placeholder="" showSuggestion="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequest"
					suggestionItems="{/ProductCollection}">
					<suggestionItems>
						<core:Item text="{Name}"/>
					</suggestionItems>
				</Input>
				<Label text="{i18n>description}" required="true"/>
				<Input placeholder="" showSuggestion="" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>workcenter}" required="true"/>
				<Input placeholder="" showSuggestion="" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>typeofvendor}" required="true"/>
				<Input placeholder="" showSuggestion="" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>suppliercrno}" required="true"/>
				<Input placeholder="" showSuggestion="" showValueHelp="" valueHelpRequest=""/>
			</fm:content>
		</fm:SimpleForm>
	</VBox>
	<VBox width="100%">
		<fm:SimpleForm title="" layout="ResponsiveGridLayout" class="ssp-form-txtarea" columnsM="2" columnsL="3" columnsXL="4"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content>
				<Label text="{i18n>Description}" required="false"/>
				<TextArea placeholder="" rows="5" cols="20" growing="true" growingMaxLines="7" height="6rem" width="100%"/>
			</fm:content>
		</fm:SimpleForm>
	</VBox>
	<VBox class="ssp-table-box">
		<!--<fm:SimpleForm id="ssp-create-sr-2" layout="ColumnLayout" class="ssp-form-bg" columnsM="2" columnsL="3" columnsXL="3"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content>-->
		<Toolbar>
			<Title level="H3" text="{i18n>itemoverview}" class="ssp-tlbr-title"/>
			<ToolbarSpacer/>
			<Button icon="sap-icon://add" press=".onAddItemsPress"/>
		</Toolbar>
		<ScrollContainer horizontal="true" vertical="false" height="auto">
			<Table mode="None" items="{path : '/MarineTransportation/itemData'}" sticky="ColumnHeaders,HeaderToolbar"
				class="ssp-request-Table sapUiContentPadding" inset="false" growingThreshold="5" growingScrollToLoad="true" growing="true"
				fixedLayout="true">
				<columns>
					<Column>
						<Text text="{i18n>shorttext}"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="{i18n>quantity}"/>
					</Column>
					<Column width="5%"></Column>
				</columns>
				<items>
					<ColumnListItem vAlign="Middle">
						<cells>
							<Input placeholder="" showSuggestion="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequest"
								suggestionItems="{/ProductCollection}">
								<suggestionItems>
									<core:Item text="{Name}"/>
								</suggestionItems>
							</Input>
							<Text text="Will shi"/>
							<Input value="{BaseUnit}"/>
							<Button icon="sap-icon://delete" press="onDeleteItemPress" type="Transparent"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</ScrollContainer>
		<!--	</fm:content>
		</fm:SimpleForm>-->
	</VBox>
	<VBox class="uploadBox sapUiSmallMarginBeginEnd">
		<upload:UploadSet busyIndicatorDelay="0" mode="None" fileSizeExceeded="onFileSizeExceed" busyIndicatorSize="Medium"
			noDataDescription="{i18n>uploadquotation}" sameFilenameAllowed="false" noDataIllustrationType="SimpleEmptyList" instantUpload="true"
			showIcons="true" terminationEnabled="true" maxFileNameLength="100" maxFileSize="5" afterItemRemoved="onFileDeleted"
			fileRenamed="onFileRenamed" fileTypes="pdf,doc,docx"
			mediaTypes="application/msword,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document" multiple="true"
			afterItemAdded="onFileAdded">
			<upload:toolbar>
				<OverflowToolbar>
					<Label text="{i18n>Attachments}" class="lbl-attach" required="true"/>
					<core:Icon src="sap-icon://message-information" size="1rem" height="3rem" tooltip="{i18n>uploadtooltip}"></core:Icon>
					<ToolbarSpacer/>
				</OverflowToolbar>
			</upload:toolbar>
		</upload:UploadSet>
	</VBox>
</core:FragmentDefinition>