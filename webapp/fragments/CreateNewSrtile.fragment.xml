<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:tnt="sap.tnt" xmlns:fm="sap.ui.layout.form" xmlns:l="sap.ui.layout"
	xmlns:core="sap.ui.core" xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds" xmlns:viz.data="sap.viz.ui5.data"
	xmlns:viz="sap.viz.ui5.controls" height="100%">
	<VBox width="100%" class="ssp-tile">
		<fm:SimpleForm id="ssp-create-sr-3" title="{i18n>Welcome User}" layout="ColumnLayout" class="ssp-form-bg" columnsM="2" columnsL="3"
			columnsXL="3" singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content></fm:content>
		</fm:SimpleForm>
		<FlexBox alignItems="Center" renderType="Div" justifyContent="Center" backgroundDesign="Solid" fitContainer="true" height="100%"
			class="sapUiResponsiveContentPadding ne-flexbox2">
			<GenericTile visible="{/TileData/Header/BPExists}" class="sapUiSmallMargin tileLayout ssp-newservice ssp-active"
				press="onPressCreateRequest" header="{i18n>createservicerequest}">
				<TileContent>
					<ImageContent src="sap-icon://add-document"/>
				</TileContent>
			</GenericTile>
			<GenericTile visible="{/TileData/Header/BPExists}" class="sapUiSmallMargin tileLayout ssp-viewservice ssp-inactive"
				press="onPressViewAllRequest" header="{i18n>viewservicerequest}">
				<TileContent>
					<ImageContent src="sap-icon://attachment-e-pub"/>
				</TileContent>
			</GenericTile>
			<GenericTile visible="{= !${/TileData/Header/BPExists}}" class="sapUiSmallMargin tileLayout ssp-viewservice ssp-inactive"
				press="onPressBpRequest" header="{i18n>createbp}">
				<TileContent>
					<ImageContent src="sap-icon://create-session"/>
				</TileContent>
			</GenericTile>
			<GenericTile class="sapUiSmallMargin tileLayout ssp-viewservice ssp-inactive" press="onPressSlaRequest" header="{i18n>createsla}">
				<TileContent>
					<ImageContent src="sap-icon://create"/>
				</TileContent>
			</GenericTile>
			<GenericTile class="sapUiSmallMargin tileLayout ssp-viewservice ssp-inactive" press="" header="{i18n>User Management}">
				<TileContent>
					<ImageContent src="sap-icon://role"/>
				</TileContent>
			</GenericTile>
		</FlexBox>
	</VBox>
	<VBox width="100%" class="ssp-tile ssp-progress sapUiMediumMarginTop">
		<fm:SimpleForm id="ssp-create-sr-4" title="{i18n>requeststatus}" layout="ColumnLayout" class="ssp-form-bg" columnsM="2" columnsL="3"
			columnsXL="3" singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content></fm:content>
		</fm:SimpleForm>
		<FlexBox alignItems="Start" justifyContent="Center" backgroundDesign="Solid" fitContainer="true" height="100%"
			class="sapUiResponsiveContentPadding">
			<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout ssp-open" header="Open" subheader="" press="onOpen"
				frameType="TwoByHalf">
				<TileContent>
					<NumericContent value="20" icon="sap-icon://open-folder" withMargin="false"/>
				</TileContent>
			</GenericTile>
			<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout ssp-progress" header="Inprogress" subheader="" press="OnInprogress"
				frameType="TwoByHalf">
				<TileContent>
					<ImageContent src=""/>
					<NumericContent value="15" icon="sap-icon://in-progress" withMargin="false"/>
				</TileContent>
			</GenericTile>
			<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout ssp-done" header="Done" subheader="" press="onDone"
				frameType="TwoByHalf">
				<TileContent>
					<ImageContent src=""/>
					<NumericContent value="10" icon="sap-icon://accounting-document-verification" withMargin="false"/>
				</TileContent>
			</GenericTile>
		</FlexBox>
	</VBox>
