<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:tnt="sap.tnt" xmlns:fm="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox width="100%">
		<fm:SimpleForm title="{i18n>createservicerequest}" layout="ResponsiveGridLayout" class="ssp-form-bg" columnsM="2" columnsL="2" columnsXL="2"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:toolbar>
				<Toolbar class="ssp-title">
					<Button press=".handleBackPress" icon="sap-icon://nav-back"/>
					<Title class="ssp-form-bg" text="{i18n>createservicerequest}"/>
					<ToolbarSpacer/>
				</Toolbar>
			</fm:toolbar>
			<fm:content>
				<Toolbar>
					<Title level="H3" text="{/ServiceDescription/}" class="ssp-tlbr-title"/>
					<ToolbarSpacer/>
					<!--<Button icon="sap-icon://settings" press=".handleButtonPress"/>-->
				</Toolbar>
				<Label text="{i18n>serviceproduct}"/>
				<Input value="{/FinanceAppVisible/}" showSuggestion="" enabled="false" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>quantity}" required="true"/>
				<Input placeholder="Enter Quantity" value="{/PMCreateRequest/Header/MaterialQty/}"/>
				<Label text="{i18n>plant}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_COMMON_SRV-/A_Plant-com.swcc.Template.fragments.CommonF4.PlantF4-/PlantF4/" width="100%"
					placeholder="Enter Plant" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/PlantF4/}">
					<customData>
						<core:CustomData key="Plant" value="Plant"/>
						<core:CustomData key="PlantName" value="Plant Name"/>
					</customData>
				</Input>
				<Label text="{i18n>shortdescription}" required="true"/>
				<Input placeholder="Allowed 40 Charcters" showSuggestion="" showValueHelp="" value="{/PMCreateRequest/Header/Descript/}" valueHelpRequest=""/>
				<core:Title text=""/>
				<Label text="{i18n>Description}" required="false"/>
				<Input placeholder="Enter Description" value="{/ServiceDescription/}" enabled="false"/>
				<Label text="{i18n>InsurancePolicyNo}" required="true"/>
				<Input
					ariaLabelledBy="-ZSSP_FI_SRV-/ZCDSV_INSURANCEVH-com.swcc.Template.fragments.FinanceModule.InsuranceClaimManagement.F4Helps.InsurancePolicyNo-/InsuranceF4/"
					width="100%" placeholder="Enter Insurance Policy No." valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/InsuranceF4/}">
					<customData>
						<core:CustomData key="Zzinspono" value="Insurance Policy No."/>
						<core:CustomData key="Zzpolicytype" value="Policy Type"/>
					</customData>
				</Input>
				<Label text="{i18n>PurchaseOrderNo}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_FI_SRV-/ZCDSV_FI_POVH2-com.swcc.Template.fragments.FinanceModule.InsuranceClaimManagement.F4Helps.POF4-/POF4/"
					width="100%" placeholder="Select PO" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/POF4/}">
					<customData>
						<core:CustomData key="PurchaseOrder" value="PO"/>
						<core:CustomData key="Supplier" value="Vendor Code"/>
					</customData>
				</Input>
			</fm:content>
			<Button text="Search" type="Default" press="onSearchMngePttyCash" width="40%" class="sapUiTinyMarginBegin sapUiContentPadding swcc-pmy-btn"/>
		</fm:SimpleForm>
	</VBox>
	<VBox width="100%">
		<fm:SimpleForm title="" layout="ResponsiveGridLayout" class="ssp-form-txtarea" columnsM="2" columnsL="3" columnsXL="4"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content>
				<Label text="{i18n>Remarks}" required="false"/>
				<TextArea placeholder="" rows="5" cols="20" text="{/PMCreateRequest/Header/NotifText/}" growing="true" growingMaxLines="7" height="6rem"
					width="100%"/>
			</fm:content>
		</fm:SimpleForm>
	</VBox>
	<VBox class="ssp-table-box">
		<!--<fm:SimpleForm id="ssp-create-sr-2" layout="ColumnLayout" class="ssp-form-bg" columnsM="2" columnsL="3" columnsXL="3"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content>-->
		<Toolbar>
			<Title level="H3" text="{i18n>Details}" class="ssp-tlbr-title"/>
			<ToolbarSpacer/>
			<Button icon="sap-icon://add" press=".onAddItemsPress"/>
		</Toolbar>
		<ScrollContainer horizontal="true" vertical="false" height="auto">
			<Table mode="None" items="{path : '/MarineTransportation/itemData'}" sticky="ColumnHeaders,HeaderToolbar"
				class="ssp-request-Table sapUiContentPadding" inset="false" growingThreshold="5" growingScrollToLoad="true" growing="true"
				fixedLayout="true">
				<columns>
					<Column>
						<Text text="Insurance Start Date"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Insurance End Date"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Plant"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Purchase Order"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Claim Value(RSA)"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Insurance Policy No."/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Claim Status"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Total Premium"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Expense"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Total Claim Value"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Accident Date"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Claim Recieved Date"/>
					</Column>
					<Column width="5%"></Column>
				</columns>
				<items>
					<ColumnListItem vAlign="Middle">
						<cells>
							<DatePicker value="" change="handleChange" class="sapUiSmallMarginBottom"/>
							<DatePicker value="" change="handleChange" class="sapUiSmallMarginBottom"/>
							<Text text="Will shi"/>
							<Text text="2"/>
							<Input value="{BaseUnit}"/>
							<Text text="2"/>
							<Text text="2"/>
							<Text text="2"/>
							<Input value="SAR"/>
							<Text text="2"/>
							<DatePicker value="{ 'path': '/valueDP11', 'type': 'sap.ui.model.type.Date'}" change="handleChange" class="sapUiSmallMarginBottom"/>
							<DatePicker value="{ 'path': '/valueDP11', 'type': 'sap.ui.model.type.Date'}" change="handleChange" class="sapUiSmallMarginBottom"/>
							<Button icon="sap-icon://delete" press="onDeleteItemPress" type="Transparent"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</ScrollContainer>
		<!--	</fm:content>
		</fm:SimpleForm>-->
	</VBox>
</core:FragmentDefinition>