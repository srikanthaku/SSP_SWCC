<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:tnt="sap.tnt" xmlns:fm="sap.ui.layout.form" xmlns:l="sap.ui.layout"
	xmlns:core="sap.ui.core">
	<VBox width="100%" class="ssp-tile">
		<fm:SimpleForm layout="ColumnLayout" class="ssp-home-bg" columnsM="2" columnsL="2" columnsXL="4" singleContainerFullSize="true"
			labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:toolbar>
				<Toolbar id="TB1" class="ssp-title">
					<Button icon="sap-icon://nav-back" press="handleBackPress"/>
					<Title text="{i18n>welcome}"/>
					<ToolbarSpacer/>
				</Toolbar>
			</fm:toolbar>
		</fm:SimpleForm>
		<FlexBox alignItems="Center" renderType="Div" justifyContent="Center" backgroundDesign="Solid" fitContainer="true" height="100%"
			class="sapUiResponsiveContentPadding ne-flexbox2 module-tile">
			<GenericTile class="sapUiSmallMargin tileLayout ssp-module" press="onPressTile" id="ZSSF" header="{i18n>FICO}">
				<TileContent value="{ path: 'modelName>/value' }">
					<ImageContent src="sap-icon://customer-financial-fact-sheet"/>
				</TileContent>
			</GenericTile>
			<GenericTile id="ZSSM" class="sapUiSmallMargin tileLayout ssp-module" press=" onPressTile" header=" {i18n>PM}">
				<TileContent>
					<ImageContent src="sap-icon://factory"/>
				</TileContent>
			</GenericTile>
			<GenericTile class="sapUiSmallMargin tileLayout ssp-module" press="onPressTile" id="ZSSS" header="{i18n>SCM}">
				<TileContent>
					<ImageContent src="sap-icon://opportunities"/>
				</TileContent>
			</GenericTile>
		</FlexBox>
		<FlexBox alignItems="Center" renderType="Div" justifyContent="Center" backgroundDesign="Solid" fitContainer="true" height="100%"
			class="sapUiResponsiveContentPadding ne-flexbox2">
			<GenericTile class="sapUiSmallMargin tileLayout ssp-module" press="onPressTile" id="ZSSH" header="{i18n>HR}">
				<TileContent>
					<ImageContent src="sap-icon://family-care"/>
				</TileContent>
			</GenericTile>
			<GenericTile class="sapUiSmallMargin tileLayout ssp-module" press="onPressTile" id="ZSSI" header="{i18n>IT}">
				<TileContent>
					<ImageContent src="sap-icon://laptop"/>
				</TileContent>
			</GenericTile>
		</FlexBox>
	</VBox>
</core:FragmentDefinition>