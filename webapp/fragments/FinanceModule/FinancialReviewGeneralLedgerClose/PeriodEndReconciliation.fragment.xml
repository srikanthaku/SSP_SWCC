							<Text text="Primary Cost or Revenue"/>
							<Text text="11411001"/>
							<Text text="42319999"/>
						</cells>
					</ColumnListItem>
					<ColumnListItem vAlign="Middle">
						<cells>
							<Text text="1000"/>
