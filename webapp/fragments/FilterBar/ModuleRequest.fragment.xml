	<HBox class="ssp-hbox" alignContent="SpaceBetween">
		<fm:SimpleForm layout="ColumnLayout" class="ssp-form-bg style-RemoveTitleHeight sapUiContentPadding" title="{i18n>modulesearch}"
			binding="{HeaderDetailsModel>/}" columnsM="2" columnsL="3" columnsXL="3" singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12"
			labelSpanM="12" labelSpanS="12">
			<Label text="{i18n>Service}" required="true"/>
			<ComboBox id="dew" width="100%" valueState="None" enabled="false">
				<items>
					<core:Item key="2" text="Finance"/>
					<core:Item key="3" text="Human Resource"/>
					<core:Item key="4" text="Information Technology"/>
					<core:Item key="5" text="Plant Maintainance"/>
					<core:Item key="6" text="Supply Chain"/>
				</items>
			</ComboBox>
			<Label text="{i18n>SelectServiceType}" required="true"/>
			<ComboBox id="idPsroductType" width="100%" valueState="None" placeholder="Select" selectedKey="{TabDetailsModel>/TabData/ProdType}"
				items="{TabDetailsModel>/ProductTypeSet}">
				<core:ListItem key="{key}" text="{text}"/>
			</ComboBox>
			<Label text="{i18n>SelectSubService}" required="true"/>
			<ComboBox id="idsubservice" width="100%" valueState="None" placeholder="Select" selectedKey="{TabDetailsModel>/TabData/ProdType}"
				items="{TabDetailsModel>/ProductTypeSet}">
				<core:ListItem key="{key}" text="{text}"/>
			</ComboBox>
			<Button text="Search" type="Default" press="onSearch" width="65%" class="sapUiTinyMarginBegin sapUiContentPadding swcc-pmy-btn"/>
		</fm:SimpleForm>
	</HBox>
</core:FragmentDefinition>