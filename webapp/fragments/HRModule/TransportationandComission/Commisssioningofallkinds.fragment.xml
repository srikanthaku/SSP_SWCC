<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:u="sap.ui.unified" xmlns:tnt="sap.tnt"
	xmlns:fm="sap.ui.layout.form" xmlns:upload="sap.m.upload" xmlns:core="sap.ui.core">
	<VBox width="100%">
		<fm:SimpleForm layout="ColumnLayout" class="ssp-form-bg" columnsM="2" columnsL="2" columnsXL="4" singleContainerFullSize="true"
			labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:toolbar>
				<Toolbar class="ssp-title">
					<Button icon="sap-icon://nav-back" press="handleBackPress"/>
					<Title class="ssp-form-bg" text="{i18n>createservicerequest}"/>
					<ToolbarSpacer/>
				</Toolbar>
			</fm:toolbar>
			<fm:content>
				<Toolbar>
					<Title level="H3" text="{/ServiceDescription/}" class="ssp-tlbr-title"/>
					<ToolbarSpacer/>
					<!--<Button icon="sap-icon://settings" press=".handleButtonPress"/>-->
				</Toolbar>
				<Label text="{i18n>serviceproduct}" required="true"/>
				<Input value="{/HRAppVisible/}" showSuggestion="" enabled="false" showValueHelp="" valueHelpRequest=""/>
				<Label text="{i18n>plant}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_COMMON_SRV-/A_Plant-com.swcc.Template.fragments.CommonF4.PlantF4-/PlantF4/" width="100%"
					placeholder="Enter Plant" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/PlantF4/}">
					<customData>
						<core:CustomData key="Plant" value="Plant"/>
						<core:CustomData key="PlantName" value="Plant Name"/>
					</customData>
				</Input>
				<Label text="{i18n>userid}" required="true"/>
				<Input enabled="false" value="{/LoginUserID}"/>
				<Label text="{i18n>Company}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_HR_SRV-/ZCDSV_SF_COMPANY-com.swcc.Template.fragments.HRModule.CompanyF4-/CompanyF4/" width="100%"
					placeholder="Select Company" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/CompanyF4/}">
					<customData>
						<core:CustomData key="keyfield" value="Company"/>
						<core:CustomData key="value_1" value="Payscale Level "/>
					</customData>
				</Input>
				<Label text="{i18n>Originstart}" required="true"/>
				<DatePicker valueFormat="dd/MM/yyyy" value="{/TransportationCommision/Commisioning/Header/Zorgdat/}" change="handleChange" class=""/>
				<Label text="{i18n>Servicedate}" required="true"/>
				<DatePicker valueFormat="dd/MM/yyyy" value="{/TransportationCommision/Commisioning/Header/Zservdat/}" change="handleChange" class=""/>
				<Label text="{i18n>Employeelocation}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_HR_SRV-/ZCDSV_SF_EMPLOC-com.swcc.Template.fragments.HRModule.EmployeeLocF4-/PayscalelevelF4/" width="100%"
					placeholder="Select Employee Loction" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/PayscalelevelF4/}">
					<customData>
						<core:CustomData key="keyfield" value="Payscale "/>
						<core:CustomData key="value_1" value="Payscale Level "/>
					</customData>
				</Input>
				<Label text="{i18n>Contracttype}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_HR_SRV-/ZCDSV_SF_CONTRACTTYPE-com.swcc.Template.fragments.HRModule.contracttypeF4-/ContracttypeF4/"
					width="100%" placeholder="Select Contract Type" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/ContracttypeF4/}">
					<customData>
						<core:CustomData key="keyfield" value="Contract Type "/>
						<core:CustomData key="value_1" value="Contract Dscription"/>
					</customData>
				</Input>
				<Label text="{i18n>payscalelevel}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_HR_SRV-/ZCDSV_SF_PAYSCALELEVEL-com.swcc.Template.fragments.HRModule.PayscalelevelF4-/PayscalelevelF4/"
					width="100%" placeholder="Select Payscale level" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/PayscalelevelF4/}">
					<customData>
						<core:CustomData key="keyfield" value="Payscale "/>
						<core:CustomData key="value_1" value="Payscale Level "/>
					</customData>
				</Input>
				<Label text="{i18n>Timeprofile}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_HR_SRV-/ZCDSV_SF_TIMEPROFILE-com.swcc.Template.fragments.HRModule.TimeProfileF4-/TimeProfileF4/" width="100%"
					placeholder="Select Time Profile" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/TimeProfileF4/}">
					<customData>
						<core:CustomData key="keyfield" value="Code "/>
						<core:CustomData key="value_1" value="Description"/>
					</customData>
				</Input>
				<Label text="{i18n>Timerecordingadmin}" required="true"/>
				<Input
					ariaLabelledBy="-ZSSP_HR_SRV-/ZCDSV_SF_TIMERECORDINGADMISS-com.swcc.Template.fragments.HRModule.TimeRecoringAdmisF4-/TimeRecoringAdmisF4/"
					width="100%" placeholder="Select Time Recording Admissibility" valueHelpOnly="false" showValueHelp="true"
					valueHelpRequest="onValueHelpRequest" value="{/TimeRecoringAdmisF4/}">
					<customData>
						<core:CustomData key="keyfield" value="Code "/>
						<core:CustomData key="value_1" value="Description"/>
					</customData>
				</Input>
				<Label text="{i18n>Clockinclockout}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_HR_SRV-/ZCDSV_SF_CLOCKINOUTGROUP-com.swcc.Template.fragments.HRModule.ClockInOutF4-/ClockInOutF4/" width="100%"
					placeholder="Select ClockIn/ClockOut" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/ClockInOutF4/}">
					<customData>
						<core:CustomData key="keyfield" value="Code "/>
						<core:CustomData key="value_1" value="Description "/>
					</customData>
				</Input>
				<Label text="{i18n>paygroup}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_HR_SRV-/ZCDSV_SF_PAYGROUP-com.swcc.Template.fragments.HRModule.PayGroupF4-/PayGroupF4/" width="100%"
					placeholder="Select Pay Group" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/PayGroupF4/}">
					<customData>
						<core:CustomData key="keyfield" value="Pay Group Code"/>
						<core:CustomData key="value_1" value="Description"/>
					</customData>
				</Input>
				<Label text="{i18n>Description}" required="true"/>
				<Input placeholder="Enter Description" value="{/ServiceDescription/}" enabled="false"/>
				<Label text="{i18n>Hiredate}" required="true"/>
				<DatePicker valueFormat="dd/MM/yyyy" value="{/TransportationCommision/Commisioning/Header/Zhiredat/}" change="handleChange" class=""/>
				<Label text="{i18n>eventreason}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_HR_SRV-/ZCDSV_SF_EVENTREASON-com.swcc.Template.fragments.HRModule.EventreasonF4-/EventreasonF4/" width="100%"
					placeholder="Select Event Reason" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/EventreasonF4/}">
					<customData>
						<core:CustomData key="keyfield" value="Event"/>
						<core:CustomData key="value_1" value="Event Reason"/>
					</customData>
				</Input>
				<Label text="{i18n>senioritystartdate}" required="true"/>
				<DatePicker valueFormat="dd/MM/yyyy" value="{/TransportationCommision/Commisioning/Header/Zseniordat/}" change="handleChange" class=""/>
				<Label text="{i18n>Position}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_HR_SRV-/ZCDSV_SF_POSITION-com.swcc.Template.fragments.HRModule.PositionF4-/PositionF4/" width="100%"
					placeholder="Select Position" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest" value="{/PositionF4/}">
					<customData>
						<core:CustomData key="keyfield" value="Position "/>
						<core:CustomData key="value_1" value="Position group"/>
					</customData>
				</Input>
				<Label text="{i18n>timezone}" required="true"/>
				<ComboBox selectedKey="{/TransportationCommision/Commisioning/Header/TimeZoneKey/}" placeholder="Select Time Zone" items="{/TimeZoneData}">
					<core:Item key="{value}" text="{label}"/>
				</ComboBox>
				<Label text="{i18n>payscale}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_HR_SRV-/ZCDSV_SF_PAYSCALEGROUP-com.swcc.Template.fragments.HRModule.PayscalegroupF4-/PayscalegroupF4/"
					width="100%" placeholder="Select Payscale Group" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/PayscalegroupF4/}">
					<customData>
						<core:CustomData key="keyfield" value="Payscale "/>
						<core:CustomData key="value_1" value="Payscale group"/>
					</customData>
				</Input>
				<Label text="{i18n>workschedule}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_HR_SRV-/ZCDSV_SF_WORKSCHEDULE-com.swcc.Template.fragments.HRModule.WorkscheduleF4-/WorkscheduleF4/"
					width="100%" placeholder="Select Work Schedule" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/WorkscheduleF4/}">
					<customData>
						<core:CustomData key="keyfield" value="Payscale "/>
						<core:CustomData key="value_1" value="Payscale group"/>
					</customData>
				</Input>
				<Label text="{i18n>timerecordingprofile}" required="true"/>
				<Input ariaLabelledBy="-ZSSP_HR_SRV-/ZCDSV_SF_TIMERECORDINGPROFILE-com.swcc.Template.fragments.HRModule.TimeRecoringF4-/TimeRecoringF4/"
					width="100%" placeholder="Select Time Recording" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/TimeRecoringF4/}">
					<customData>
						<core:CustomData key="keyfield" value="Code "/>
						<core:CustomData key="value_1" value="Description"/>
					</customData>
				</Input>
				<Label text="{i18n>timerecordingvariant}" required="true"/>
				<Input
					ariaLabelledBy="-ZSSP_HR_SRV-/ZCDSV_SF_TIMERECORDINGVARIANT-com.swcc.Template.fragments.HRModule.TimeRecoringVariantF4-/TimeRecoringVariantF4/"
					width="100%" placeholder="Select Time Recording Variant" valueHelpOnly="false" showValueHelp="true" valueHelpRequest="onValueHelpRequest"
					value="{/TimeRecoringVariantF4/}">
					<customData>
						<core:CustomData key="keyfield" value="Payscale "/>
						<core:CustomData key="value_1" value="Payscale group"/>
					</customData>
				</Input>
				<Label text="{i18n>payrollid}" required="true"/>
				<Input placeholder="Enter Payroll ID" showSuggestion="" value="{/TransportationCommision/Commisioning/Header/Payrollid/}" showValueHelp=""
					valueHelpRequest=""/>
			</fm:content>
		</fm:SimpleForm>
	</VBox>
	<VBox width="100%">
		<fm:SimpleForm title="" layout="ResponsiveGridLayout" class="ssp-form-txtarea" columnsM="2" columnsL="3" columnsXL="4"
			singleContainerFullSize="true" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12">
			<fm:content>
				<Label text="{i18n>Remarks}" required="false"/>
				<TextArea placeholder="" rows="5" cols="20" growing="true" growingMaxLines="7" height="6rem" width="100%"/>
			</fm:content>
		</fm:SimpleForm>
	</VBox>
	<VBox>
		<Table items="{/UploadedData}" class="upload-data" noDataText="{i18n>uploadquotation}">
			<headerToolbar>
				<Toolbar>
					<content>
						<FlexBox class="sapUiTinyMarginTop" alignItems="Start" justifyContent="SpaceBetween">
							<Title text="{i18n>Attachments}" level="H1"></Title>
							<Text text="{i18n>uploadtooltip}" class="sapUiTinyMarginBegin upload-info">
								<layoutData>
									<FlexItemData growFactor="1"/>
								</layoutData>
							</Text>
							<u:FileUploader class="sapUiContentPadding" buttonOnly="true" multiple="true" icon="sap-icon://attachment" visible="true"
								sameFilenameAllowed="false" buttonText="Upload file" change="onFileAdded" filenameLengthExceed="onFileNameLengthExceed"
								fileSizeExceed="onFileSizeExceed" maximumFileSize="2" uploadComplete="onFileComplete1" typeMissmatch="handleMissmatch"
								placeholder="Choose a file" style="Emphasized"
								mimeType="application/msword,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document"/>
						</FlexBox>
					</content>
				</Toolbar>
			</headerToolbar>
			<columns>
				<Column>
					<Text text="File Name"/>
				</Column>
				<Column>
					<Text text="File Size"/>
				</Column>
				<Column>
					<Text text="Delete"/>
				</Column>
				<!-- Add more columns as needed -->
			</columns>
			<items>
				<ColumnListItem>
					<cells>
						<Text text="{Filename}"/>
						<Text text="{path:'Filesize',formatter:'.formatBytesToMB'}"/>
						<Button type="Reject" text="Delete" press="onDeleteAttachment"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</VBox>
</core:FragmentDefinition>